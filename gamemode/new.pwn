	case 40: slot = 12;
	default: slot = -1;
	}
	return slot;
}
public OnPlayerWeaponShot(playerid, weaponid, hittype, hitid, Float:fX, Float:fY, Float:fZ)
{
	if(IsAAntidm(hitid)) return 0;
	if(weaponid < 0 || weaponid > 54) return OnPlayerCheat(playerid,91);
	if(weaponid != 38)
	{
		if(GetPVarInt(playerid,"FastShot")>=10) return OnPlayerCheat(playerid,3);
		if(GetTickCount() - shotTime[playerid] < 60 && (weaponid == 24 || weaponid == 25)) return OnPlayerCheat(playerid,4);
		if(GetTickCount() - shotTime[playerid] < 60) SetPVarInt(playerid,"FastShot",GetPVarInt(playerid,"FastShot")+1);
		if(GetPlayerAmmo(playerid)-10 > GunInfo[playerid][pAmmo][Slot(weaponid)]) return OnPlayerCheat(playerid,5);
		if(weaponid != GunInfo[playerid][pWeapon][Slot(weaponid)]) OnPlayerCheat(playerid,6);
		GunInfo[playerid][pAmmo][Slot(weaponid)] --;
	}
    if(!IsPlayerInAnyVehicle(playerid)) switch(GetPlayerWeapon(playerid)) {
    case WEAPON_COLT45, WEAPON_DEAGLE, WEAPON_SHOTGUN, WEAPON_SAWEDOFF, WEAPON_SHOTGSPA, WEAPON_UZI, WEAPON_MP5, WEAPON_M4, WEAPON_AK47, WEAPON_TEC9, WEAPON_RIFLE, WEAPON_SNIPER, WEAPON_ROCKETLAUNCHER, WEAPON_HEATSEEKER, WEAPON_FLAMETHROWER, WEAPON_MINIGUN: whcheck[playerid] = false;
    default: whcheck[playerid] = true, KickEx(playerid); }
	return shotTime[playerid] = GetTickCount();
}
