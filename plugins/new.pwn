public OnPlayerTakeDamage(playerid, issuerid, Float:amount, weaponid)
{
	if(issuerid != INVALID_PLAYER_ID)
	{
		GetPlayersDamage[issuerid] ++;
		if(weaponid == 34) SetPlayerHealth(playerid, 0);
		GetPlayerHealth(playerid, HealthDamageid[playerid]);
		if(weaponid != 34) HealthDamageid[playerid] -= amount;
		if(GetPVarInt(issuerid, "GetStrike") == 0 && testintiniteammo == true) SetPVarInt(issuerid, "GetStrike", GetTickCount()); // 1 выстрел, включаем тик.
		if((GetTickCount() - GetPVarInt(issuerid, "GetStrike")) <= 50 && HealthDamageid[playerid] <= 0.0 && GreenWeapons(weaponid) && PlayerInfo[issuerid][pAdmin] < 3 && GetPlayerState(issuerid) != PLAYER_STATE_PASSENGER && GetPVarInt(issuerid, "RezervAmmo") == 0)
		{
			if(RezervAmmo[issuerid][weaponid] == GetPlayerAmmo(issuerid) && testintiniteammo == true)
			{
				SetPVarInt(issuerid, "WeaponDamage", weaponid);
				SetPVarInt(issuerid, "RezervAmmo", gettime()+1);
				return true;
			}
		}
		else SetPVarInt(issuerid, "GetStrike", GetTickCount()), RezervAmmo[playerid][weaponid] = GetPlayerAmmo(issuerid); // 2 выстрел снова включаем тик проверяем патроны
		if(HealthDamageid[playerid] <= 0.0 && GreenWeapons(weaponid) && PlayerInfo[issuerid][pAdmin] < 3 && GetPlayerState(issuerid) != PLAYER_STATE_PASSENGER && GetPVarInt(issuerid, "LastAmmo") == 0)
		{
			if(LastAmmo[issuerid][weaponid] == GetPlayerAmmo(issuerid))
			{
				SetPVarInt(issuerid, "WeaponDamage", weaponid);
				SetPVarInt(issuerid, "LastAmmo", gettime()+1);
				return true;
			}
		}
		LastAmmo[issuerid][weaponid] = GetPlayerAmmo(issuerid);
	}
	return true;
}
