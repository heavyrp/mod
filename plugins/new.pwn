public OnPlayerUpdate(playerid)
{
	NoRoof(playerid);
	PlayerInfo[playerid][pAFK] = 0;
	if(IsPlayerInVehicle( playerid, GetPlayerVehicleID ( playerid )) && ( GetPlayerWeapon ( playerid ) == WEAPON_DEAGLE  ))// Если в машине у игрока в руках дигл
	{
		SetPlayerArmedWeapon(playerid, 0);// то выставляем слот оружия на кулаки
	}
	if(GetPVarInt(playerid, "BuyCar") == 1)
	{
		format(strings,128,"~n~~n~~n~~n~~n~~n~~n~~n~~n~~b~Model: ~w~%s~n~~b~Cost: ~w~$%d~n~~b~Color 1: ~w~%d ~b~Color 2: ~w~%d", VehicleNames[vehiclebuy[playerid]-400], buymoneyveh[playerid], scolor[playerid], scolor1[playerid]);
		GameTextForPlayer(playerid, strings, 2000, 3);
	}
	if(Gunoperation[playerid] == 0)
	{
		new weapons[13][2];
		for (new i = 0; i < 13; i++)
		{
			GetPlayerWeaponData(playerid, i, weapons[i][0], weapons[i][1]);
			if(Fixanticheat[playerid] == true)
			{
				format(strings, 64, "%d: %d (%d)", i, weapons[i][0], weapons[i][1]);
				SendClientMessage(playerid, COLOR_GRAD1, strings);
			}
			if(weapons[i][0] >= 1 && weapons[i][0] != 4 && weapons[i][0] <= 8 && !IsPlayerInAnyVehicle(playerid) && weapons[i][1] > 0)
			{
				if(Gunscharger[playerid][i] != weapons[i][0])
				{
					Delgun(playerid);
					CheatKick(playerid,"0030");
					return true;
				}
			}
			if(weapons[i][0] > 15 && weapons[i][0] != 46 && weapons[i][0] != 40 && weapons[i][0] < 47 && weapons[i][1] > 0)
			{
				if(Gunscharger[playerid][i] != weapons[i][0])
				{
					if(Fixanticheat[playerid] == true) {}
					Delgun(playerid);
					CheatKick(playerid,"0031");
					return true;
				}
			}
			if(weapons[i][1] != Ammocharger[playerid][i] && CheckFireArm(weapons[i][0]))
			{
				if(weapons[i][1] < Ammocharger[playerid][i])
				{
					Ammocharger[playerid][i] = weapons[i][1];
				}
				else if((weapons[i][0] >= 16 && weapons[i][0] <= 18) || (weapons[i][0] >= 22 && weapons[i][0] <= 39) || (weapons[i][0] >= 41 && weapons[i][0] <= 43))
				{
					Delgun(playerid);
					CheatKick(playerid,"0032");
					return true;
				}
			}
		}
	}
	if(GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_USEJETPACK) return CheatKick(playerid,"0028");
	return 1;
}
forward OnPlayerUpdateTimer();
public OnPlayerUpdateTimer()
{
	foreach(Player,playerid)
	{
		if(gPlayerLogged[playerid] != 1) return true;
		iNewPlayerAmmo[playerid] = GetPlayerAmmo(playerid);
		if(iNewPlayerAmmo[playerid] != iPlayerAmmo[playerid])
		{
			OnPlayerAmmoChange(playerid, iNewPlayerAmmo[playerid], iPlayerAmmo[playerid]);
			iPlayerAmmo[playerid] = iNewPlayerAmmo[playerid];
		}
		if(TeleportTime[playerid] > 0) TeleportTime[playerid]--;
		GetPlayerPos(playerid, TelePos[playerid][3], TelePos[playerid][4], TelePos[playerid][5]);
		if(TelePos[playerid][5] > 550.0){TelePos[playerid][0] = 0.0; TelePos[playerid][1] = 0.0;}
		if(GetTickCount() - LagServer[playerid] > 2000){TelePos[playerid][0] = 0.0; TelePos[playerid][1] = 0.0;}
		if(GetPlayerSurfingVehicleID(playerid) != INVALID_VEHICLE_ID){TelePos[playerid][0] = 0.0; TelePos[playerid][1] = 0.0;}
		if(TelePos[playerid][0] != 0.0)
		{
			new Float:xdist = TelePos[playerid][3]-TelePos[playerid][0];
			new Float:ydist = TelePos[playerid][4]-TelePos[playerid][1];
			new Float:sqxdist = xdist*xdist;
			new Float:sqydist = ydist*ydist;
			new Float:distance = (sqxdist+sqydist)/31;
			if(distance < 1600 && distance > 140 && TeleportTime[playerid] == 0 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER && PlayerInfo[playerid][pAdmin] < 1 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING && Cheater[playerid] == 0 && WaitCheat == 0) return CheatKick(playerid,"0022");
			if(distance < 1600 && distance > 80 && TeleportTime[playerid] == 0 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && PlayerInfo[playerid][pAdmin] < 1 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING && Cheater[playerid] == 0 && WaitCheat == 0) return CheatKick(playerid,"0021");
			if(distance > 800 && TeleportTime[playerid] == 0 && gPlayerSpawned[playerid] == 1 && PlayerInfo[playerid][pAdmin] < 1 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING && Cheater[playerid] == 0 && WaitCheat == 0) return CheatKick(playerid,"0020");
			new dgun = GetPlayerWeapon(playerid);
			if(dgun == 23 || dgun == 24 || dgun == 25 || dgun == 29 || dgun == 30 || dgun == 31 || dgun == 33 || dgun == 38)
			{
				if(GetPlayerAmmo(playerid) == 0 && GetPlayerWeapon(playerid) > 1 && gPlayerLogged[playerid] == 1 && !IsPlayerInAnyVehicle(playerid) && GetPlayerWeaponState(playerid) == 2)
				{
					CheatKick(playerid,"0019");
					Delgun(playerid);
				}
			}
			if(!IsPlayerInAnyVehicle(playerid)) if(!IsPlayerInAnyVehicle(playerid) && PlayerInfo[playerid][pAdmin] < 7)
			{
				new Float:Z;
				GetPlayerPos(playerid,Z,Z,Z);
				new anim = GetPlayerAnimationIndex(playerid);
				if((anim == 1538 || anim == 1539 || anim == 1543) && Z > 2)
				{
					if(!NotFlyingHack(playerid))
					{
						if(PlayerInfo[playerid][pAdmin] > 2) return true;
						Delgun(playerid);
						ResetPlayerWeapons(playerid);
						CheatKick(playerid,"0015");
					}
				}
			}
			if(GetPlayerSkin(playerid) == 74)
			{
				PlayerInfo[playerid][pModel] = 1;
				PlayerInfo[playerid][pChar] = 1;
				SetPlayerSkin(playerid, 1);
				CheatKick(playerid,"0018");
			}
			if(IDVEH[playerid] != -1 && IsPlayerConnected(playerid) && IsPlayerInAnyVehicle(playerid) && UseEnter[playerid] != true && IDVEH[playerid] != GetPlayerVehicleID(playerid) && PlayerInfo[playerid][pAdmin] < 1) return CheatKick(playerid,"0017");
			new Float:xw, Float:yw, Float:zw;// Античит на пиздарванку
			GetPlayerVelocity(playerid,xw,yw,zw);
			if(xw > 2.0 || yw > 2.0)
			{
				SetPVarInt(playerid, "Pizdorvanka", GetPVarInt(playerid, "Pizdorvanka") + 1);
				if(GetPVarInt(playerid, "Pizdorvanka") >= 3)
				{
					if(PlayerInfo[playerid][pAdmin] == 0)
					{
						SetPVarInt(playerid, "Pizdorvanka", 0);
						CheatKick(playerid,"228");
					}
				}
			}
			//if(GetPlayerState(playerid) == PLAYER_STATE_SPECTATING && PlayerInfo[playerid][pAdmin] < 1) return CheatKick(playerid,"0016");
			if(IsPlayerInAnyVehicle(playerid) && PlayerInfo[playerid][pAdmin] < 1 && GetVehicleModel(GetPlayerVehicleID(playerid)) == 462 && SpeedVehicle(playerid) > 200) return CheatKick(playerid,"0015");
			new Float:AntibronNew; // Античит на бронь
			GetPlayerArmour(playerid, AntibronNew);
			if(PlayerInfo[playerid][pMember] <= 0 && AntibronNew > 50 && PlayerInfo[playerid][pAdmin] < 1)
			{
				SetPlayerArmour(playerid, 0);
				CheatKick(playerid,"0013");
			}
			if(PlayerInfo[playerid][pAdmin] < 1 && acstruct[playerid][checkmaptp] == 1)
			{
				new Float:dis = GetPlayerDistanceFromPoint(playerid, acstruct[playerid][maptp][0], acstruct[playerid][maptp][1], acstruct[playerid][maptp][2]);
				if(dis < 5.0)
				{
					new Float:disd = GetPlayerDistanceFromPoint(playerid, acstruct[playerid][LastOnFootPosition][0], acstruct[playerid][LastOnFootPosition][1], acstruct[playerid][LastOnFootPosition][2]);
					if(disd > 25.0) return CheatKick(playerid,"0022");
				}
				acstruct[playerid][checkmaptp] = false;
			}
		}
		GetPlayerPos(playerid, acstruct[playerid][LastOnFootPosition][0], acstruct[playerid][LastOnFootPosition][1], acstruct[playerid][LastOnFootPosition][2]);
		if(TelePos[playerid][5] < 550.0 && TelePos[playerid][3] != 0.0){TelePos[playerid][0] = TelePos[playerid][3]; TelePos[playerid][1] = TelePos[playerid][4];}
		LagServer[playerid] = GetTickCount();
	}
	return true;
}

stock CheatKick(id,code[])
{
	if(IsPlayerInAnyVehicle(id)) SetVehicleToRespawn(GetPlayerVehicleID(id));
	new zCheatString[256];
	format(zCheatString, sizeof(zCheatString), "Вы были кикнуты по подозрению в читерстве! (#%s)",code);
	SendClientMessage(id, 0xFF6347AA, zCheatString);
	format(strings,72,"Античит < ERROR %s > кикнут(а) %s",code,PlayerInfo[id][pNames]);
	SendAdminMessage(COLOR_GREY,strings);
	SendClientMessage(id,COLOR_LIGHTRED,"Введите (/q)uit чтобы выйти");
	SetTimerEx("PlayerKick",300,false,"d", id);
	return true;
}
stock FixHour(hour)
{
	hour = timeshift+hour;
	if (hour < 0) { hour = hour+24; }
	else if (hour > 23) { hour = hour-24; }
	shifthour = hour;
	return true;
}
