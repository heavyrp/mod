		if(SBizzInfo[h][sbMafia] <= 0)
		{
			SBizzInfo[h][sbMafia] = Player[playerid][pMember];
			SendClientMessage(playerid, COLOR_WHITE, "Бизнес взят под контроль");
			return true;
		}
