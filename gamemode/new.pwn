									SendClientMessage(params[0], COLOR_GREY, "Если вы случайно нажали 'Enter' и меню пропало, нажмите 'Enter' еще раз");
									SetPlayerInterior(params[0],5);
									Player[params[0]][pInt] = 5;
									SetPlayerVirtualWorld(params[0],1);
									ShowMenuForPlayer(ChoseSkin,params[0]);
									new rand = random(sizeof(gInviteSpawns));
									SetPlayerPos(params[0], gInviteSpawns[rand][0], gInviteSpawns[rand][1], gInviteSpawns[rand][2]); // Warp the player
									SetPlayerFacingAngle(params[0], gInviteSpawns[rand][3]);
									SetPlayerCameraPos(params[0],gInviteSpawns[rand][0] + 3, gInviteSpawns[rand][1], gInviteSpawns[rand][2]);
									SetPlayerCameraLookAt(params[0],gInviteSpawns[rand][0], gInviteSpawns[rand][1], gInviteSpawns[rand][2]);
									TogglePlayerControllable(params[0], 0);
									SelectChar[params[0]] = INVALID_PLAYER_ID;
									Player[params[0]][pJob] = 0;
									SelectCharID[params[0]] = Player[params[0]][pMember];
									SelectCharPlace[params[0]] = 1;
									Player[params[0]][pModel] = InviteSkin[params[0]];
									SetPlayerSkin(params[0], InviteSkin[params[0]]);
									return true;
								}
								Player[params[0]][pRank] = 0;
								Player[params[0]][pMember] = 0;
								Player[params[0]][pLeader] = 0;
								SetPlayerArmourAC(params[0],0);
								SpawnPlayerAC(params[0]);
								Delgun(params[0]);
								ResetPlayerWeapons(params[0]);
								format(stringer1, sizeof(stringer1), "Агент FBI %s уволил вас из армии", Player[playerid][pNames]);
								SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
								format(stringer1, sizeof(stringer1), "Вы снова гражданский", Player[playerid][pNames]);
								SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
								format(stringer1, sizeof(stringer1), "Вы уволили %s из армии.", Player[params[0]][pNames]);
