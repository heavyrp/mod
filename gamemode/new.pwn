stock CaptureEnd(familyone, familytwo)
{
	foreach (new i : Player)
	{
		if(!IsAGang(i)) continue;
		if(Player[i][pMember] == familyone || Player[i][pMember] == familytwo)
		{
			PlayerTextDrawHide(i, BandaCapt1a[i]);
			PlayerTextDrawHide(i, BandaCapt2a[i]);
			PlayerTextDrawHide(i, ScoreCapt1a[i]);
			PlayerTextDrawHide(i, ScoreCapt2a[i]);
			PlayerTextDrawHide(i, CaptTime[i]);
			PlayerTextDrawHide(i, Textdraw4[i]);
			for(new num; num < 10; num++) SendDeathMessageToPlayer(i,6000,5005, 255);
			SetPVarInt(i,"GzOn",0);
		}
	}
}
stock CaptureUpd(bandkiller)
{
	new SCORE1[128],SCORE2[128];
	foreach (new i : Player)
	{
		if(!IsAGang(i)) continue;
		if(GangInfo[Player[i][pMember]][capture] > 0)
		{
			if(GangInfo[bandkiller][gangnumber] == 1)
			{
				format(SCORE1, 64, "%d", GangInfo[bandkiller][score]);
				PlayerTextDrawSetString(i,ScoreCapt1a[i], SCORE1);
			}
			else
			{
				format(SCORE2, 64, "%d", GangInfo[bandkiller][score]);
				PlayerTextDrawSetString(i,ScoreCapt2a[i], SCORE2);
			}
		}
	}
}
