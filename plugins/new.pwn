				format(stringer, sizeof(stringer), "%s согласился быть вашим свидетелем на свадьбе.", PlayerInfo[playerid][pNames]);
				SendClientMessage(MarryWitnessOffer[playerid], 0x6ab1ffaa, stringer);
				MarryWitness[MarryWitnessOffer[playerid]] = playerid;
				MarryWitnessOffer[playerid] = INVALID_PLAYER_ID;
			}
			else SendClientMessage(playerid, COLOR_WHITE, "Игрок, который просил, чтобы вы были свидетелем не рядом с вами !");
		}
		else SendClientMessage(playerid, COLOR_WHITE, "Вам не предлагали быть свидетелем!");
	}
	else if(strcmp(param, "marriage",true) == 0)
	{
		if(ProposeOffer[playerid] != INVALID_PLAYER_ID)
		{
			if(!PlayerToPoint(10.0, playerid, -1988.6638,1117.8837,54.4726)) return   SendClientMessage(playerid, COLOR_WHITE, "Вы не у церкви в Сан-Фиерро!");
			if(IsPlayerConnected(ProposeOffer[playerid]))
			{
				if(ProxDetectorS(10.0, playerid, ProposeOffer[playerid]))
				{
					if(MarryWitness[ProposeOffer[playerid]] == INVALID_PLAYER_ID) return SendClientMessage(playerid, COLOR_WHITE, "У него/неё нет свидетеля!");
					if(ProxDetectorS(12.0, ProposeOffer[playerid], MarryWitness[ProposeOffer[playerid]]))
					{
						new stringer[150];
						format(stringer, sizeof(stringer), "Вы приняли предложение от %s's", PlayerInfo[ProposeOffer[playerid]][pNames]);
						SendClientMessage(playerid, 0x6ab1ffaa, stringer);
						format(stringer, sizeof(stringer), "%s приняла Ваш запрос быть Вашей Женой.", PlayerInfo[playerid][pNames]);
						SendClientMessage(ProposeOffer[playerid], 0x6ab1ffaa, stringer);
						format(stringer, sizeof(stringer), "{FC6DEC}::: Поздравляем! %s и %s теперь муж и жена! :::", PlayerInfo[playerid][pNames], PlayerInfo[ProposeOffer[playerid]][pNames]);
						SendClientMessageToAll(COLOR_WHITE,stringer);
						PlayerInfo[ProposeOffer[playerid]][pCash] -= 100000;
						PlayerInfo[playerid][pMarried] = 1;
						PlayerInfo[ProposeOffer[playerid]][pMarried] = 1;
						MarryWitness[ProposeOffer[playerid]] = INVALID_PLAYER_ID;
						ProposeOffer[playerid] = INVALID_PLAYER_ID;
					}
					else SendClientMessage(playerid, COLOR_WHITE, "Нет рядом свидетелей!");
				}
				else SendClientMessage(playerid, COLOR_WHITE, "Жених/Невеста не рядом с Вам!");
			}
		}
		else SendClientMessage(playerid, COLOR_WHITE, "Вам ни кто не предлагал!");
	}
	else if(strcmp(param, "taxi",true) == 0)
	{
		if(TransportDuty[playerid] != 1) return   SendClientMessage(playerid, COLOR_WHITE, "Вы не таксист!");
		if(TaxiCall != INVALID_PLAYER_ID)
		{
			new stringer[128];
			format(stringer, sizeof(stringer), "Вы приняли вызов от %s.",PlayerInfo[TaxiCall][pNames]);
			SendClientMessage(playerid, 0x6ab1ffaa, stringer);
			format(stringer, sizeof(stringer), "Водитель такси %s принял ваш вызов, оставайтесь на месте.",PlayerInfo[playerid][pNames]);
			SendClientMessage(TaxiCall, COLOR_GREEN, stringer);
			TaxiCall = INVALID_PLAYER_ID;
		}
		else SendClientMessage(playerid, COLOR_WHITE, "Нет Вызовов");
	}
	else if(strcmp(param, "ticket",true) == 0)
	{
		if(TicketOffer[playerid] != INVALID_PLAYER_ID)
		{
			if (ProxDetectorS(5.0, playerid, TicketOffer[playerid]))
