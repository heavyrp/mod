stock SpeedVehicle(playerid)
{
	new Float:ST[4];
	if(IsPlayerInAnyVehicle(playerid)) GetVehicleVelocity(GetPlayerVehicleID(playerid),ST[0],ST[1],ST[2]);
	else GetPlayerVelocity(playerid,ST[0],ST[1],ST[2]);
	ST[3] = floatsqroot(floatpower(floatabs(ST[0]), 2.0) + floatpower(floatabs(ST[1]), 2.0) + floatpower(floatabs(ST[2]), 2.0)) * 213.3;
	return floatround(ST[3]);
}
public OnVehicleDamageStatusUpdate(vehicleid, playerid)
{
	new Float:carhp;
	GetVehicleHealth(vehicleid, carhp);
	foreach(Player, i)
	{
		if(GetPlayerVehicleID(playerid) == caridhouse[i])
		{
			if(carhp > 700 && carhp < 1000)
			{
				PlayerInfo[playerid][pProz] -= 0;
				return true;
			}
			else if(carhp > 500 && carhp < 700)
			{
				PlayerInfo[playerid][pProz] -= 1;
				return true;
			}
			else if(carhp > 200 && carhp < 500)
			{
				if(PlayerInfo[playerid][pProz] > 0)
				{
					PlayerInfo[playerid][pProz] -= 2;
					return true;
				}
			}
		}
	}
	return true;
}
stock CheckHealth()
{
	new Float: Health;
	foreach(Player,i) // Цикл, проверяем всех игроков онлайн
	{
		if(IsPlayerConnected(i))
		{
			GetPlayerHealth(i, Health);
			if(PlayerHealth[i] < Health) SetPlayerHealthAC(i, PlayerHealth[i]);
			else PlayerHealth[i] = Health;
		}
	}
	return true;
}
GetWeaponSlot(weaponid)
{
	switch(weaponid)
	{
	case 0, 1: return 0;
	case 2..9: return 1;
	case 22..24: return 2;
	case 25..27: return 3;
	case 28, 29, 32: return 4;
	case 30, 31: return 5;
	case 33, 34: return 6;
	case 35..38: return 7;
	case 16..18, 39: return 8;
	case 41..43: return 9;
	case 10..15: return 10;
	case 44..46: return 11;
	case 40: return 12;
	default: return 0;
	}
	return 0;
}

CheckFireArm(weaponid)
{
	switch(weaponid)
	{
	case 16..39, 41..43: return 1;
	}
	return 0;
}
stock GiveWeapon(playerid,weaponid,ammo)
{
	Gunoperation[playerid] = 2;
	new slot = GetWeaponSlot(weaponid);
	Gunscharger[playerid][slot] = weaponid;
	Ammocharger[playerid][slot] += ammo;
	LastAmmo[playerid][weaponid] += ammo;
	RezervAmmo[playerid][weaponid] += ammo;
	GivePlayerWeapon(playerid,weaponid,ammo);
	Gunoperation[playerid] = 2;
