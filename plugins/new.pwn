CMD:klyap(playerid, params[])
{
	if(sscanf(params,"d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /klyap [id]");
	if(IsPlayerConnected(params[0]))
	{
		if(params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Нельзя у самого себя вытащить кляп.");
		if(PlayerTied[params[0]] == 0)  return SendClientMessage(playerid, COLOR_WHITE, "Игрок не связан!");
		if(PlayerTied[playerid] > 0)  return SendClientMessage(playerid, COLOR_WHITE, "Вы связаны");
		new stringer[150];
		if (ProxDetectorS(8.0, playerid, params[0]))
		{
			if(gag[params[0]] > 0)
			{
				gag[params[0]] = 0;
				format(stringer, sizeof(stringer), "%s вытащил кляп у %s'a", PlayerInfo[playerid][pNames] ,PlayerInfo[params[0]][pNames]);
				ProxDetector(30.0, playerid, stringer, 0xDD90FFFF,0xDD90FFFF,0xDD90FFFF,0xDD90FFFF,0xDD90FFFF);
			}
			else
			{
				if(!ISAMAFIA(playerid)) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Вам недоступна данная команда.");
				gag[params[0]] = 1;
				TieTime[params[0]] = 600;
				format(stringer, sizeof(stringer), "%s вставил кляп в рот %s'у", PlayerInfo[playerid][pNames] ,PlayerInfo[params[0]][pNames]);
				ProxDetector(30.0, playerid, stringer, 0xDD90FFFF,0xDD90FFFF,0xDD90FFFF,0xDD90FFFF,0xDD90FFFF);
			}
		}
		else SendClientMessage(playerid, COLOR_WHITE, "Человек не рядом с вами!");
	}
	else SendClientMessage(playerid, COLOR_WHITE, "Игрок оффлайн!");
	return true;
}
CMD:klyapoff(playerid, params[])
