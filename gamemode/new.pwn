public OnVehicleMod(playerid,vehicleid,componentid)
{
	if(componentid!=1010 && GetPlayerInterior(playerid)==0)
	{
		RemoveVehicleComponent(vehicleid,componentid);
		SetVehicleToRespawn(vehicleid);
		KickEx(playerid);
		return true;
	}
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER && vehicleid == Player[playerid][pCar])
	{
		Player[playerid][pSpoiler] = GetVehicleComponentInSlot(vehicleid, CARMODTYPE_SPOILER);
		Player[playerid][pHood] = GetVehicleComponentInSlot(vehicleid, CARMODTYPE_HOOD);
		Player[playerid][pBamper1]  = GetVehicleComponentInSlot(vehicleid, CARMODTYPE_FRONT_BUMPER);
		Player[playerid][pBamper2] = GetVehicleComponentInSlot(vehicleid, CARMODTYPE_REAR_BUMPER);
		Player[playerid][pGidra] = GetVehicleComponentInSlot(vehicleid, CARMODTYPE_HYDRAULICS);
		Player[playerid][pNitro] = GetVehicleComponentInSlot(vehicleid, CARMODTYPE_NITRO);
		Player[playerid][pExhaust] = GetVehicleComponentInSlot(vehicleid, CARMODTYPE_EXHAUST);
		Player[playerid][pLamps] = GetVehicleComponentInSlot(vehicleid, CARMODTYPE_LAMPS);
		Player[playerid][pVozdyx] = GetVehicleComponentInSlot(vehicleid, CARMODTYPE_ROOF);
		Player[playerid][pKolesocar] = GetVehicleComponentInSlot(vehicleid, CARMODTYPE_WHEELS);
		Player[playerid][pSideskirt] = GetVehicleComponentInSlot(vehicleid, CARMODTYPE_SIDESKIRT);
	}
	return true;
}

public OnVehiclePaintjob(playerid, vehicleid, paintjobid)
{
	if(paintjobid>2)
	{
		ChangeVehiclePaintjob( vehicleid, 0 );
		SetVehicleToRespawn(vehicleid);
		KickEx(playerid);
		return true;
	}
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER && vehicleid == Player[playerid][pCar]) Player[playerid][pPokraska] = paintjobid;
	return true;
}

