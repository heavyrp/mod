CMD:amembers(playerid,params[])
{
	if(Player[playerid][pAdmin] < 5) return true;
	if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "•[{FF0000}Подсказка{FFFFFF}]•:{D0D0D0} /amembers [1-26]");
