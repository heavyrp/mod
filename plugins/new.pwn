	new rows, fields, preg[64], maximum[64];
	cache_get_data(rows, fields);
	if(!rows)
	{
		SendClientMessage(playerid, COLOR_GREY,"Аккаунт не найден!");
	}
	else if(rows)
	{
		cache_get_field_content(0, "pEmail", maximum), strmid(preg, maximum, 0, strlen(maximum), 64);
