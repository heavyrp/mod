stock StartPaintball()
{
	PaintBallGo++;
	if(PaintBallGo == 180) OOCOff(0xFAC3F6AA, "Внимание! Начало пейнтболла через 2 минуты. Место проведения: Военный завод K.A.C.C");
	if(PaintBallGo == 300)
	{
		PaintballRound = 1;
		PaintballWinnerKills = 0;
		PaintballWinner = 999;
		foreach(Player, i)
		{
			if(PaintballPlayers < 1)
			{
				OOCOff(0xFAC3F6AA, "Внимание! Матч по пейнтболлу отменен из - за не достаточного количества участников");
				PaintballRound = 0;
				PlayerPaintballing[i] = false;
				PaintballWinnerKills = 0;
				Timer[1] = 0;
				PaintBallGo = 0;
				return true;
			}
			if(PlayerPaintballing[i] != false)
			{
				PaintballRab = 1;
				TextDrawShowForPlayer(i, PaintBallTime[i]);
				new rand;
				SetPlayerHealthAC(i, 100);
				SetPlayerArmourAC(i, 100);
				Delgun(i);
				ResetPlayerWeapons(i);
				SetPlayerColor(i, COLOR_REDD);
				GiveWeapon(i, 24, 21);
				SetPlayerInterior(i,0);
				SetPlayerVirtualWorld(i, 100);
				rand = random(sizeof(PaintBallaspawns));
				SetPPos(i, PaintBallaspawns[rand][0], PaintBallaspawns[rand][1], PaintBallaspawns[rand][2]);
			}
		}
		OOCOff(0xFAC3F6AA, "Внимание! Матч Пейнтболл начался");
	}
	if(PaintBallGo == 600)
	{
		foreach(Player, i)
		{
			if(PlayerPaintballing[i] != false)
			{
				if(IsPlayerConnected(PaintballWinner))
				{

					format(strings, 90, "{FFFFFF}%s выйграл матч по пейнтболлу с {ffa500}%d {FFFFFF}убийствами", PlayerInfo[PaintballWinner][pNames], PlayerPaintballKills[PaintballWinner]);
					SendClientMessage(i, COLOR_YELLOW2, strings);
					PlayerInfo[PaintballWinner][pCash] +=250000;
					GameTextForPlayer(PaintballWinner, "~g~+$250000", 5000, 1);
					PaintballWinnerKills = 0;
				}
				TextDrawHideForPlayer(i, PaintBallTime[i]);
				Delgun(i);
				ResetPlayerWeapons(i);
				PlayerPaintballing[i] = false;
				SetPlayerInterior(i,7);
				SetPlayerVirtualWorld(i, 34);
				SetPPos(i,307.1729,-142.0335,1004.0625);
				SetPlayerHealthAC(i, 100.0);
				SetPlayerArmourAC(i, 0);
				PlayerInfo[i][pCash] += 5000;
				PlayerPaintballKills[i] = 0;
				PlayerPaintballDeaths[i] = 0;
				SetPlayerToTeamColor(i);
			}
		}
		Timer[1] = 0;
		PaintballRab = 0;
		PaintballWinnerKills = 0;
		PaintballWinner = 999;
		PaintballRound = 0;
		PaintBallGo = 0;
		PaintballPlayers = 0;
		OOCOff(0xFAC3F6AA, "Внимание! Пейнтболл закончен.");
	}
	foreach(Player, i)
	{
		if(PlayerPaintballing[i] != false)
		{
			if(PaintballRab == 1)
			{
				new numbers = 600 - PaintBallGo;
				format(strings, 130, "~g~Time: ~w~%s~n~~n~~w~Kills: ~g~%d~n~~r~Deaths: ~g~%d", Convert(numbers), PlayerPaintballKills[i], PlayerPaintballDeaths[i]);
				TextDrawSetString(PaintBallTime[i], strings);
			}
		}
	}
	return true;
}

