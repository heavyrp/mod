	case 22..24: slot = 2;
	case 25..27: slot = 3;
	case 28,29,32: slot = 4;
	case 30,31: slot = 5;
	case 33,34: slot = 6;
	case 35..38: slot = 7;
	case 16..18,39: slot = 8;
	case 41..43: slot = 9;
