			SBizzInfo[b][sTill] += cenabenz;
			PlayerInfo[playerid][pCash] -= cenabenz;
			SBizzInfo[b][sBenz] -= floatround(benz);
			Fuell[params[0]] = 300;
			GameTextForPlayer(playerid,"~w~~n~~n~~n~~n~~n~~n~~n~~n~~n~Please Wait",2000,3);
		}
	}
	return true;
}
