				}
				if(HouseInfo[h][hKlass] == 1 && HouseInfo[h][hOwned] == 1)//Классы домов
				{
					HouseInfo[h][hTakings] -=600; kazna += 600;
					if(HouseInfo[h][hTakings] > 33600) HouseInfo[h][hTakings] = 33600;
				}
