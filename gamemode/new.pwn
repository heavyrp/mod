public OnPlayerSelectedMenuRow(playerid, row)
{
	//printf("Игрок %s[%d] использовал меню.",Player[playerid][pNames],playerid);
	new Menu:Current = GetPlayerMenu(playerid);
	if(Current == Admin)
	{
		switch(row)
		{
		case 0:
			{
				new param_str[11];
				format(param_str, sizeof(param_str), "%d", SpecAd[playerid]);
				cmd::recon(playerid, param_str);
				ShowMenuForPlayer(Admin,playerid);
			}
		case 1:
			{
				if(Player[SpecAd[playerid]][pMuted] == 0)
				{
					format(stringer1, sizeof(stringer1), "Администратор {FF0000}%s {FFFFFF}дал бан чата игроку {FF0000}%s{FFFFFF}.",Player[playerid][pNames],Player[SpecAd[playerid]][pNames]);
					SendClientMessageToAll(COLOR_WHITE, stringer1);
					Player[SpecAd[playerid]][pMuteTime] = 120;
					Player[SpecAd[playerid]][pMuted] = 1;
					ShowMenuForPlayer(Admin,playerid);
				}
				else
				{
					format(stringer1, sizeof(stringer1), "Администратор {FF0000}%s {FFFFFF}снял бан чата у {FF0000}%s{FFFFFF}.",Player[playerid][pNames],Player[SpecAd[playerid]][pNames]);
					SendClientMessageToAll(COLOR_WHITE, stringer1);
					Player[SpecAd[playerid]][pMuteTime] = 0;
					Player[SpecAd[playerid]][pMuted] = 0;
					ShowMenuForPlayer(Admin,playerid);
				}
			}
		case 2:
			{
				SendClientMessage(playerid, COLOR_WHITE, "Используй /kick");
				ShowMenuForPlayer(Admin,playerid);
			}
		case 3:
			{
				SendClientMessage(playerid, COLOR_WHITE, "Используй /warn");
				ShowMenuForPlayer(Admin,playerid);
			}
		case 4:
			{
				SendClientMessage(playerid, COLOR_WHITE, "Используй /ban");
				ShowMenuForPlayer(Admin,playerid);
			}
		case 5:
			{
				new Float:shealth, Float:slx, Float:sly, Float:slz;
				GetPlayerHealth(SpecAd[playerid], shealth);
				SetPlayerHealthAC(SpecAd[playerid], shealth-5);
				GetPlayerPos(SpecAd[playerid], slx, sly, slz);
				SetPlayerPos(SpecAd[playerid], slx, sly, slz+5);
				PlayerPlaySound(SpecAd[playerid], 1130, slx, sly, slz+5);
				format(stringer1, sizeof(stringer1), "Админ %s дал пинка %s",Player[playerid][pNames],Player[SpecAd[playerid]][pNames]);
				ABroadCast(COLOR_LIGHTRED,stringer1,1);
				SendClientMessage(SpecAd[playerid], COLOR_LIGHTRED, stringer1);
				ShowMenuForPlayer(Admin,playerid);
			}
		case 6:
			{
				new param_str[11];
				format(param_str, sizeof(param_str), "%d", SpecAd[playerid]);
				cmd::getstats(playerid, param_str);
				ShowMenuForPlayer(Admin,playerid);
			}
		case 7:
			{
				new param_str[11];
				format(param_str, sizeof(param_str), "%d", SpecAd[playerid]);
				cmd::getip(playerid, param_str);
				ShowMenuForPlayer(Admin,playerid);
			}
		case 8:
			{
				new param_str[11];
				format(param_str, sizeof(param_str), "%d", SpecAd[playerid]);
				cmd::gm(playerid, param_str);
				ShowMenuForPlayer(Admin,playerid);
			}
		case 9:
			{
				if(GetPlayerState(playerid) == PLAYER_STATE_SPECTATING)
				{
					ShowMenuForPlayer(Admin,playerid);
					StopSpectate(playerid);
					SetPlayerHealthAC(playerid, 100);
				}
			}
		}
	}
