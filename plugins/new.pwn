public OnPlayerSpawn(playerid)
{
	gunsellergun[playerid] = 0, gunsellerpatr[playerid] = 0, gunsellercost[playerid] = 0, realgun[playerid] = 0,gunsellerid[playerid] = INVALID_PLAYER_ID;
	SetPlayerScore(playerid, PlayerInfo[playerid][pLevel]);
	StopAudioStreamForPlayer(playerid);
	SetPVarInt(playerid,"K_Times",0);
	SetPVarInt(playerid,"AirTime",3);
	SetPVarInt(playerid, "AnttiBunnyHopping", 1);
	KillTimer(STimer[playerid]);
	TeleportTime[playerid] = 4;
	SetPlayerSpawn(playerid);
	if(PlayerInfo[playerid][pTut] == 0)
	{
		new rand = random(sizeof(gInviteSpawns));
		SetPPos(playerid, gInviteSpawns[rand][0], gInviteSpawns[rand][1], gInviteSpawns[rand][2]); // Warp the player
		SetPlayerFacingAngle(playerid, gInviteSpawns[rand][3]);
		SetPlayerCameraPos(playerid,gInviteSpawns[rand][0] + 3, gInviteSpawns[rand][1], gInviteSpawns[rand][2]);
		SetPlayerCameraLookAt(playerid,gInviteSpawns[rand][0], gInviteSpawns[rand][1], gInviteSpawns[rand][2]);
		SetPlayerInterior(playerid,5);
		TogglePlayerControllable(playerid, false);
		new skin =2 + random(100);
		SetPlayerVirtualWorld(playerid,skin);
	}
	if(PlayerPaintballing[playerid] == false && PaintballRab == 0)
	{
		SetPlayerHealthAC(playerid, PlayerInfo[playerid][pHP]);
	}
	if(GetPVarInt(playerid, "spectate_admin"))
	{
		DeletePVar(playerid, "spectate_admin");
		SetPPos(playerid, GetPVarFloat(playerid, "PosSpecX"), GetPVarFloat(playerid, "PosSpecY"), GetPVarFloat(playerid, "PosSpecZ"));
		SetPlayerInterior(playerid, GetPVarInt(playerid, "SpecInt"));
		SetPlayerVirtualWorld(playerid, GetPVarInt(playerid, "SpecVT"));
		SetPlayerHealthAC(playerid, PlayerInfo[playerid][pHP]);
		HideReconMenuForPlayer(playerid);
		HideReconPlayerInfoForPlayer(playerid);
		TogglePlayerSpectating(playerid, 0);
		if(!IsPlayerConnected(GetPVarInt(playerid, "SpecID"))) GameTextForPlayer(playerid, "~w~RECON ~r~OFF~n~PLAYER DISCONNECT", 1500, 4);
		DeletePVar(GetPVarInt(playerid, "SpecID"), "spectate_player");
		DeletePVar(playerid, "SpecID");
		return true;
	}
	return true;
}
