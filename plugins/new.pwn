			{
				format(strings, sizeof(strings), "[%d] %s ранг: %d{FFFFFF}", i, PlayerInfo[i][pNames], PlayerInfo[i][pRank]);
				SendClientMessage(playerid, 0xC0E1EEFF, strings);
			}
		}
	}
	if(IsACop(playerid) || IsAArm(playerid) || IsAMayor(playerid) || IsAMedic(playerid) || PlayerInfo[playerid][pMember] == 11 || PlayerInfo[playerid][pMember] == 9 || PlayerInfo[playerid][pMember] == 16 || PlayerInfo[playerid][pMember] == 20)
	SendClientMessage(playerid, COLOR_LIGHTRED, "Выходные:");
	foreach(Player, i)
	{
		if(IsPlayerConnected(i))
		{
			if(PlayerInfo[i][pLeader] == params[0] && FormaFrac[i] == 0)
			{
				format(strings, sizeof(strings), "[%d] %s возглавляет организацию{FFFFFF}", i, PlayerInfo[i][pNames]);
				SendClientMessage(playerid, 0xC0E1EEFF, strings);
			}
			else if(PlayerInfo[i][pMember] == params[0] && FormaFrac[i] == 0)
			{
				format(strings, sizeof(strings), "[%d] %s ранг: %d{FFFFFF}", i, PlayerInfo[i][pNames], PlayerInfo[i][pRank]);
				SendClientMessage(playerid, 0xC0E1EEFF, strings);
			}
		}
	}
	if(PlayerInfo[playerid][pLeader] != 0) SendClientMessage(playerid, COLOR_WHITE, "Члены организации оффлайн: /offmembers");
