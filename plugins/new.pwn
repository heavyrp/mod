stock SendCode(playerid, email[])
{
	PlayerInfo[playerid][pCode] = random(9999);
	new message[256];
	format(message, sizeof(message), "Здравствуйте, %s!\nДля привязки E-Mail к вашему игровому аккаунту, введите код подтверждения в диалоговое окно игры.\n\nВаш код для подтверждения регистрации: %d", PlayerInfo[playerid][pNames], PlayerInfo[playerid][pCode]);
	SendMail(email, "support@sil-roleplay.tk", "SIL Role Play", "sil-roleplay.tk", message);
	return true;
}
publics: GetBannedConnect(playerid)
{
	new rows, fields;
	cache_get_data(rows, fields);
	if(rows)
	{
		SendClientMessage(playerid,0xC22000AA," Ваш IP адрес заблокирован");
		KickEx(playerid);
	}
	else
	{
		mysql_format(connects, strings, 128, "SELECT * FROM `accounts` WHERE name = '%e' Limit 1", PlayerInfo[playerid][pNames]);
		mysql_function_query(connects, strings, true, "GetAccount", "i", playerid);
	}
	return true;
}

publics: GetBannedIp(playerid, ip[])
{
	new rows, fields;
	cache_get_data(rows, fields);
	if(rows) SendClientMessage(playerid, COLOR_GREY, "IP адрес уже заблокирован");
	else
	{
		mysql_format(connects, strings, 60, "INSERT INTO `banip` (`IP`) VALUES ('%s')", ip);
		mysql_query(strings, -1, -1, connects);
		format(strings, 70, "%s забанил IP: %s", PlayerInfo[playerid][pNames], ip);
		SendAdminMessage(COLOR_LIGHTRED,strings);
		format(strings,sizeof(strings),"banip %s",ip);
		SendRconCommand(strings);
		SendRconCommand("reloadbans");
	}
	return true;
}

publics: GetUnBannedIp(playerid, ip[])
{
	new rows, fields;
	cache_get_data(rows, fields);
	if(rows)
	{
		mysql_format(connects, strings, 52, "DELETE FROM `banip` WHERE IP = '%s'", ip);
		mysql_query(strings, -1, -1, connects);
		format(strings, 75, "%s разбанил IP: %s", PlayerInfo[playerid][pNames], ip);
		SendAdminMessage(COLOR_GREEN,strings);
		format(strings,sizeof(strings),"unbanip %s",ip);
