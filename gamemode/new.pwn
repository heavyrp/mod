CMD:contract(playerid, params[])
{
	if(IsPlayerConnected(playerid))
	{
		if(sscanf(params, "dd",params[0], params[1])) return SendClientMessage(playerid, COLOR_WHITE, " Введите: /contract [id] [сумма]");
		if(Player[params[0]][pAdmin] > 7) return SendClientMessage(playerid, COLOR_GREY, "Вы не можете заказать данного человека.");
		if(params[1] < 10000 || params[1] > 10000000) return SendClientMessage(playerid, COLOR_GREY, "Сумма контракта должна быть минимум $10000, и максимум $1000000!");
		if (IsPlayerConnected(params[0]))
		{
			if(params[0] != INVALID_PLAYER_ID)
			{
				if(Player[playerid][pMember] == 23) return SendClientMessage(playerid,COLOR_GREY,"Вы не можете заказать человека.");
				if(Player[params[0]][pMember] == 23) return SendClientMessage(playerid,COLOR_GREY,"Вы не можете заказать данного человека.");
				if(IsACop(params[0]) && params[1] < 50000) return SendClientMessage(playerid, COLOR_GREY,"Голова данного человека стоит больше чем вы предлогаете.");
				if(params[0] == playerid) return SendClientMessage(playerid, COLOR_GREY, "Вы не можете заказать себя!");
				if(Player[params[0]][pContract] > 0) return SendClientMessage(playerid,COLOR_GREY,"Данная жертва уже была заказана, хитманы не могут её принять.");
				if(Player[playerid][pCash] < params[1]) return SendClientMessage(playerid,COLOR_GREY,"Вы решили нас обмануть? У вас нету денег!");
				Player[playerid][pCash] -= params[1];
				Player[params[0]][pHeadValue] += params[1];
				format(stringer1, sizeof(stringer1), "Внимание, поступил новый заказ цена головы заказчика %d вирт.", params[1]);
				SendFamilyMessage(23, COLOR_YELLOW, stringer1);
				SendMes(playerid,COLOR_WHITE,"Вы успешно заказали %s за %d вирт.",Player[params[0]][pNames],params[1]);
				Player[params[0]][pContract] = 600;
			}
		}
	}
	return true;
}
