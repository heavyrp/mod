Convert(number)
{
	new hours = 0, mins = 0, secs = 0, string[30];
	hours = floatround(number / 3600);
	mins = floatround((number / 60) - (hours * 60));
	secs = floatround(number - ((hours * 3600) + (mins * 60)));
	if(hours > 0) format(string, 40, "%d:%02d:%02d", hours, mins, secs);
	else format(string, 40, "%d:%02d", mins, secs);
	return string;
}
stock IsPlayerInPerimeter(playerid, vertices, Float:XCoords[], Float:YCoords[])
{
	new u = vertices-1, Intersect = 0, Float:X, Float:Y, Float:Z; GetPlayerPos(playerid,X,Y,Z);
	for(new v = 0; v < vertices; v++)
	{
		if((YCoords[v] < Y && YCoords[u] >= Y) || (YCoords[u] < Y && YCoords[v] >= Y))
		{
			if(XCoords[v] + (Y-YCoords[v]) / (YCoords[u] - YCoords[v]) * (XCoords[u] - XCoords[v]) < X)
			{
				Intersect++;
			}
		}
		u = v;
	}
	return Intersect&1;
}
