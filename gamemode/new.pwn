			case 1: nado = 60;
			case 2: nado = 120;
			case 3: nado = 180;
			case 4: nado = 240;
			}
			format(stringer, sizeof(stringer), "Ваш скил автоугонщика: %d. Для повышения надо сдать еще %d машин.", skilles, nado - Player[playerid][pAutoSkill]);
