CMD:tipster(playerid, params[])
{
	if(PlayerInfo[playerid][pMember] != 2) return true;
	if(FormaFrac[playerid] == 0) return SendClientMessage(playerid, COLOR_WHITE, "Вам нужно начать рабочий день.");
	new param[40];
	if(sscanf(params, "s[32]S()[64]", param, params))
	{
		SendClientMessage(playerid,-1," Введите: /tipster [действие]");
		SendClientMessage(playerid, COLOR_WHITE, " Доступные названия:");
		SendClientMessage(playerid, COLOR_WHITE, "  GET - Взять жучок.");
		SendClientMessage(playerid, COLOR_WHITE, "  SET - Прикрепить жучок к игроку.");
		SendClientMessage(playerid, COLOR_WHITE, "  REMOVE - Деактивировать жучок.");
		SendClientMessage(playerid, COLOR_WHITE, "  LISTEN - Начать прослушивание чата фракции.");
		return true;
	}
	else if(strcmp(param, "get",true) == 0)
	{
		if(GetPlayerSkin(playerid) == PlayerInfo[playerid][pModel]) return SendClientMessage(playerid, COLOR_WHITE, "Вам не дали возможность шпионить.");
		if(GetPlayerVehicleID(playerid) >= fbicar[0] && GetPlayerVehicleID(playerid) <= fbicar[1])
		{
			if(tipster == true) return SendClientMessage(playerid,COLOR_WHITE, "Жучок активирован.");
			tipsterr[playerid] = true;
			tipster = true;
			format(strings, 80, "%s взял(а) жучок", PlayerInfo[playerid][pNames]);
			SendFamilyMessage(2, 0x00b953ff, strings);
		}
		else SendClientMessage(playerid,COLOR_WHITE, "Необходимо находиться в автомобиле ФБР.");
	}
	else if(strcmp(param, "set",true) == 0)
	{
		if(GetPlayerSkin(playerid) == PlayerInfo[playerid][pModel]) return SendClientMessage(playerid, COLOR_WHITE, "Вам не дали возможность шпионить.");
		new ammo;
		if(sscanf(params, "u", ammo)) return SendClientMessage(playerid, COLOR_WHITE, " Введите: /tipster set [ид игрока]");
		if(PlayerInfo[ammo][pAdmin] != 0 || PlayerInfo[ammo][pMember] == 2 || PlayerInfo[ammo][pMember] == 0) return SendClientMessage(playerid, COLOR_WHITE, "Игрок не состоит в организации.");
		if(tipsterr[playerid] == false) return SendClientMessage(playerid, COLOR_WHITE, "Вы не взяли жучок.");
		if(tipster == false) return SendClientMessage(playerid, COLOR_WHITE, "Жучок не активирован.");
		if(tipsterOn[playerid] == true) return SendClientMessage(playerid, COLOR_WHITE, "У вас нет жучка.");
		if(!ProxDetectorS(2.0, playerid, ammo)) return SendClientMessage(playerid, COLOR_WHITE, "Игрок не рядом с вами.");
		if(PlayerInfo[ammo][pMember] == 0) return SendClientMessage(playerid, COLOR_WHITE, "Игрок не состоит в организации.");
		tipsterOn[playerid] = true;
		tipsters[playerid] = PlayerInfo[ammo][pMember];
		tipsterlisten = ammo;
		format(strings, 128, "%s установил(а) жучок на %s. Чтобы подключиться к волне, введите /tipster listen", PlayerInfo[playerid][pNames], PlayerInfo[ammo][pNames]);
		SendFamilyMessage(2, 0x00b953ff, strings);
		SetPVarInt(playerid, "Tipster", gettime()+1); // Запустили таймер на 1 секунду
	}
	else if(strcmp(param, "listen",true) == 0)
	{
		if(tipsterlisten == -1) return SendClientMessage(playerid,COLOR_WHITE,"Жучок не активирован.");
		if(!tipsterl[playerid])
		{
			if(GetDistanceBetweenPlayers(playerid, tipsterlisten) > 200) return SendClientMessage(playerid,COLOR_WHITE,"Жучок не обнаружен.");
			tipsterl[playerid] = true;
			tipsterl2[playerid] = true;
			SendClientMessage(playerid, 0x00b953ff, "Вы начали прослушивание");
		}
		else
		{
			tipsterl[playerid] = false;
			tipsterl2[playerid] = false;
			SendClientMessage(playerid, 0x00b953ff, "Вы завершили прослушивание");
		}
	}
	else if(strcmp(param, "remove",true) == 0)
	{
		new ammo;
		if(tipster == false) return SendClientMessage(playerid,COLOR_WHITE,"Жучок не активирован.");
		if(PlayerInfo[playerid][pRank] < 6) { if(!tipsterr[playerid] == false) return SendClientMessage(playerid, COLOR_WHITE, "Вы не взяли жучок."); }
		SetPVarInt(playerid, "Tipster", 0); // Завершаем работу таймера
		tipster = false;
		tipsterr[playerid] = false;
		tipsterl[playerid] = false;
		tipsterl2[playerid] = false;
		tipsterOn[playerid] = false;
		tipsterfail[playerid] = false;
		ammo = tipsterlisten;
		tipsterlisten = -1;
		tipsters[playerid] = -1;
		format(strings, 80, "%s деактивировал(а) жучок", PlayerInfo[playerid][pNames]);
		SendFamilyMessage(2, 0x00b953ff, strings);
		SendClientMessage(ammo , 0x00b953ff, "Жучок деактивирован");
	}
	return true;
}
CMD:contract(playerid, params[])
{
	if(PlayerInfo[playerid][pMember] == 23) return SendClientMessage(playerid, COLOR_WHITE, YouCanNot);
	if(sscanf(params,"ud", params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, " Введите: /contract [id игрока] [сумма]");
	if(!IsPlayerConnected(params[0]) || params[0] == INVALID_PLAYER_ID) return SendClientMessage(playerid, COLOR_WHITE, PlayerIsNotOnline);
	if(PlayerInfo[params[0]][pMember] == 23 && PlayerInfo[playerid][pMember] == 23) return SendClientMessage(playerid, COLOR_GREY, " * Нельзя заказать контракт на члена вашего агенства!");
	if(PlayerInfo[playerid][pLevel] < 2) return SendClientMessage(playerid, COLOR_GREY, " * Необходимо быть 2 уровнем, чтобы класть контракты.");
	if(params[0] == playerid) return SendClientMessage(playerid, COLOR_GREY, " * Вы не можете заказать себя!");
	if(params[1] < 10000 || params[1] > 1000000) return SendClientMessage(playerid, COLOR_GREY, "   Сумма котрактов должна быть минимум $10000, и максимум $1000000!");
