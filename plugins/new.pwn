CMD:usedrugs(playerid, params[])
{
	if(sscanf(params,"d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, " Введите: /usedrugs [количество]");
	if(GetPVarInt(playerid,"Narko") > gettime()) return  SendClientMessage(playerid,COLOR_GREY,"{FF0000}x {AFAFAF}Нельзя слишком часто употреблять наркотик!");
	switch(PlayerInfo[playerid][pNarcoLomka])
	{
	case 0..1999: if(params[0] > 3 || params[0] < 1) return SendClientMessage(playerid, COLOR_GREY, "{FF0000}x {AFAFAF}Нельзя меньше 1 грамма и больше 3 грамм!");
	case 2000..4999: if(params[0] > 12 || params[0] < 1) return SendClientMessage(playerid, COLOR_GREY, "{FF0000}x {AFAFAF}Нельзя меньше 1 грамма и больше 12 грамм!");
	default: if(params[0] > 15 || params[0] < 1) return SendClientMessage(playerid, COLOR_GREY, "{FF0000}x {AFAFAF}Нельзя меньше 1 грамма и больше 15 грамм!");
	}
	if(PlayerInfo[playerid][pDrugs] <= params[0]) return SendClientMessage(playerid, COLOR_GREY, "{FF0000}x {AFAFAF}Недостаточно наркотиков");
	SetPVarInt(playerid,"Narko", gettime()+60);
	startnarko[playerid] = 0;
	send[playerid] = 0;
	if(PlayerInfo[playerid][pNarcoLomka] <= 4000)
	{
		SetPlayerWeather(playerid, -68);
		SetPlayerTime(playerid,17,0);
		SetPVarInt(playerid,"NarkoTime", gettime()+10);
	}
	if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
	{
		if(PlayerCuffed[playerid] == 0) ApplyAnimation(playerid,"SMOKING","M_smk_drag",4.1,0,0,0,0,0,1);
	}
	new Float:health;
	GetPlayerHealth(playerid, health);
	SetPlayerHealthAC(playerid, health + 10.0*float(params[0]));
	switch(PlayerInfo[playerid][pNarcoLomka])
	{
	case 0..1999: if(PlayerInfo[playerid][pHP] >= 120.0) SetPlayerHealthAC(playerid, 120.0);
	case 2000..2999: if(PlayerInfo[playerid][pHP] >= 130.0) SetPlayerHealthAC(playerid, 130.0);
	case 3000..3999: if(PlayerInfo[playerid][pHP] >= 140.0) SetPlayerHealthAC(playerid, 140.0);
	case 4000..4999: if(PlayerInfo[playerid][pHP] >= 150.0) SetPlayerHealthAC(playerid, 150.0);
	default: if(PlayerInfo[playerid][pHP] >= 160.0) SetPlayerHealthAC(playerid, 160.0);
	}
	PlayerInfo[playerid][pDrugs] -= params[0];
	PlayerInfo[playerid][pNarcoLomka] += params[0]*6;
	format(strings, sizeof(strings), "(( Здоровье пополнено до: %0.0f ))", PlayerInfo[playerid][pHP]);
	SendClientMessage(playerid, COLOR_WHITE, strings);
	format(strings, 42, "%s употребил(а) наркотик", PlayerInfo[playerid][pNames]);
	ProxDetector(10.0, playerid, strings, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
	return true;
}
