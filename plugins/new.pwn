public UpdateTimers()
{
	new GetTime = gettime();
	if(Timer[1] <= GetTime && Timer[1] != 0)
	{
		Timer[1] = GetTime+1; // Снова запустили таймер. Он будет повторяющийся
		StartPaintball(); // Выполняем действие
	}
	for(new A, B = GetMaxPlayers(); A != B; A++)
	{
		if(GetPVarInt(A, "Bunny") <= GetTime && GetPVarInt(A, "Bunny") != 0)
		{
			SetPVarInt(A, "Bunny", 0);
			AntiBH(A);
		}
		if(GetPVarInt(A, "Tipster") <= GetTime && GetPVarInt(A, "Tipster") != 0)
		{
			SetPVarInt(A, "Tipster", GetTime+1); // Снова запустили таймер. Он будет повторяющийся
			TipsterPlayer(A); // Выполняем действие
		}
		if(GetPVarInt(A, "NarkoTime") <= GetTime && GetPVarInt(A, "NarkoTime") != 0)
		{
			SetPVarInt(A, "NarkoTime", 0); // Завершаем работу таймера
			NarkoTimer(A); // Выполняем действие
		}
		if(GetPVarInt(A, "CarHacker") <= GetTime && GetPVarInt(A, "CarHacker") != 0)
		{
			SetPVarInt(A, "CarHacker", 0); // Завершаем работу таймера
			CarHack(A, GetPVarInt(A, "GetCar")); // Выполняем действие
		}
		if(GetPVarInt(A, "LastAmmo") <= GetTime && GetPVarInt(A, "LastAmmo") != 0)
		{
			SetPVarInt(A, "LastAmmo", 0);
			AntiCheatSeven(A);
		}
		if(GetPVarInt(A, "RezervAmmo") <= GetTime && GetPVarInt(A, "RezervAmmo") != 0)
		{
			SetPVarInt(A, "RezervAmmo", 0);
			AntiCheatSeven2(A);
		}
		if(GetPVarInt(A, "AdmGM") <= GetTime && GetPVarInt(A, "AdmGM") != 0)
		{
			SetPVarInt(A, "AdmGM", 0);
			UpdateProverka(A);
		}
	}
	return true;
}
