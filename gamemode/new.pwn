stock AntiCheat(i)
{
	new Float:Z,Float:hp;
	new tttgtg,str[300];
	tttgtg = GetPlayerAmmo(SpecAd[i]);
	new Float:hppp,Float:aermor,Float:health;
	GetPlayerArmour(SpecAd[i],aermor);
	GetPlayerHealth(SpecAd[i],hppp);
	GetVehicleHealth(GetPlayerVehicleID(SpecAd[i]),health);
	new fps = 40 + random(60);
	if(Player[i][pAdmin] >= 1)
	{
		if(IsPlayerInAnyVehicle(SpecAd[i]))
		{
			GetPlayerIp(SpecAd[i],playersip,sizeof(playersip));
			format(str, sizeof(str),"~g~%s %i~n~~n~~w~Number: %i~n~IP: %s~n~~n~Ping: %i~n~~r~Speed: ~w~%i~n~~g~Armour: ~w~%.0f~n~~g~Health: ~w~%.0f~n~Warns: %i~n~~b~Ammo: ~w~%i~n~~n~~g~$ ~w~Cash: ~g~%i~n~~g~$ ~w~Bank: ~g~%i~n~~n~~r~Scan FPS: %i~n~Car HP: %.0f",Player[SpecAd[i]][pNames],
			SpecAd[i],Player[SpecAd[i]][pPnumber],playersip,GetPlayerPing(SpecAd[i]),SpeedVehicle(SpecAd[i]),aermor,hppp,Player[SpecAd[i]][pWarns],tttgtg,Player[SpecAd[i]][pCash],Player[SpecAd[i]][pBank],fps,health);
			PlayerTextDrawSetString(i,FULLRECON[i],str);
		}
		if(!IsPlayerInAnyVehicle(SpecAd[i]))
		{
			GetPlayerIp(SpecAd[i],playersip,sizeof(playersip));
			format(str, sizeof(str),"~g~%s %i~n~~n~~w~Number: %i~n~IP: %s~n~~n~Ping: %i~n~~r~Speed: ~w~%i~n~~g~Armour: ~w~%.0f~n~~g~Health: ~w~%.0f~n~Warns: %i~n~~b~Ammo: ~w~%i~n~~n~~g~$ ~w~Cash: ~g~%i~n~~g~$ ~w~Bank: ~g~%i~n~~n~~r~Scan FPS: %i",Player[SpecAd[i]][pNames],SpecAd[i],Player[SpecAd[i]][pPnumber],playersip,GetPlayerPing(SpecAd[i]),SpeedVehicle(SpecAd[i]),aermor,
			hppp,Player[SpecAd[i]][pWarns],tttgtg,Player[SpecAd[i]][pCash],Player[SpecAd[i]][pBank],fps);
			PlayerTextDrawSetString(i,FULLRECON[i],str);
		}
	}
	//AntiCheat
	//if(!spec[i] && GetPlayerState(i) == PLAYER_STATE_SPECTATING) OnPlayerCheat(i,51);
	if(GetPlayerAnimationIndex(i) == 1231 && GetPlayerWeapon(i) != 46) OnPlayerCheat(i,53);
	//Anti-AirBreak
	if(GetPlayerDistanceFromPoint(i,TelePosSpawn[i][0],TelePosSpawn[i][1],TelePosSpawn[i][2]) > 10 && GetPlayerDistanceFromPoint(i,TelePos[i][0],TelePos[i][1],TelePos[i][2]) >= 20 && GetPlayerState(i)==PLAYER_STATE_ONFOOT && !spec[i] && !WaitAirCheat[i] && !InAFK(i)) AddCheater(i,0);
	if(GetPlayerDistanceFromPoint(i,TelePos[i][0],TelePos[i][1],TelePos[i][2]) >= 20 && VehicleSpeed(GetPlayerVehicleID(i)) < 3 && GetPlayerState(i)==2 && !WaitAirCheat[i] && !InAFK(i)) AddCheater(i,0);
	GetPlayerPos(i, TelePos[i][0], TelePos[i][1], TelePos[i][2]);
	if(WaitAirCheat[i]) WaitAirCheat[i]--;
	//
	if(GetPlayerSpecialAction(i) == SPECIAL_ACTION_USEJETPACK) OnPlayerCheat(i,22);
	if(GetPlayerState(i)!=PLAYER_STATE_DRIVER) return true;
	//
	GetVehicleHealth(GetPlayerVehicleID(i),hp); GetVehiclePos(GetPlayerVehicleID(i),Z,Z,Z);
	if(hp > VehicleHealth[i] && GetPlayerState(i) == 2) AddCheater(i,1);
	GetVehicleHealth(GetPlayerVehicleID(i),VehicleHealth[i]);
	if(GetPVarFloat(i,"PosZ")-5 > Z || GetPVarFloat(i,"PosZ")+5 < Z) SetPVarFloat(i,"PosZ",Z);
	if(VehicleSpeed(GetPlayerVehicleID(i)) >= 135) OnPlayerCheat(i,102);
	return true;
}
stock OnPlayerCheat(playerid,result)
{
	if(!IsPlayerConnected(playerid)) return 1;
	if(Player[playerid][pAdmin] >= 2 && (result < 4 || result > 6)) return 1;
	SendMes(playerid,COLOR_RED,"Вы кикнуты по подозрению в читерстве, код: #%i",result), KickEx(playerid);
	format(stringer1, sizeof(stringer1), "Игрок: %s[%d] был кикнуты за использования постороннего ПО {F3EEEE}(код: #%i)",Player[playerid][pNames],playerid,result);
	SendAdminMessage(COLOR_GREY,stringer1);
	return 1;
}
