	}
	antiwarn[playerid] += 8;
	AntiBan[playerid] += 30;
	if(antiwarn[playerid] >= 30 && anti[playerid] == 0) anti[playerid] = 1, antiwarn[playerid] = 300;
	format(strings, 128 , "Администратор: %s кикнул игрока %s. Причина: %s", PlayerInfo[playerid][pNames],PlayerInfo[params[0]][pNames], params[1]);
