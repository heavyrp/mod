	if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) { if(PlayerCuffed[playerid] == 0 && GetPVarInt(playerid, "AntiDMAnim") != 1) ApplyAnimation(playerid,"COP_AMBIENT","Coplook_watch",4.1,0,0,0,0,0); }
