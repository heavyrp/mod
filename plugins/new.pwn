	new mtext[20];
	new year, month,day;
	getdate(year, month, day);
	switch(month)
	{
	case 1: mtext = "January";
	case 2: mtext = "February";
	case 3: mtext = "March";
	case 4: mtext = "April";
	case 5: mtext = "May";
	case 6: mtext = "June";
	case 7: mtext = "July";
	case 8: mtext = "August";
	case 9: mtext = "September";
	case 10: mtext = "October";
	case 11: mtext = "November";
	case 12: mtext = "December";
	}
	new hour,minuite,second;
	gettime(hour,minuite,second);
	FixHour(hour);
	hour = shifthour;
	if (minuite < 10)
	{
		if(PlayerInfo[playerid][pJailTime] > 0) format(strings, 90, "~y~%d %s~n~~g~~w~%d:0%d~n~~w~Jail Time Left: %d sec", day, mtext, hour, minuite, PlayerInfo[playerid][pJailTime]-10);
		else format(strings, 45, "~y~%d %s~n~~g~~w~%d:0%d", day, mtext, hour, minuite);
	}
	else
	{
		if(PlayerInfo[playerid][pJailTime] > 0) format(strings, 90, "~y~%d %s~n~~g~~w~%d:%d~n~~w~Jail Time Left: %d sec", day, mtext, hour, minuite, PlayerInfo[playerid][pJailTime]-10);
		else format(strings, 45, "~y~%d %s~n~~g~~w~%d:%d", day, mtext, hour, minuite);
