CMD:id(playerid, params[])
{
	new tmp[64];
	if(sscanf(params, "s[64]", tmp)) return SendClientMessage(playerid, -1, "Введите: /id [имя игрока]");
	SendClientMessage(playerid,0x6495EDFF,"Игроки онлайн:");
	new id=-1,name[30];
	if(!isNumeric(tmp))
	{
		foreach(Player, i)
		{
			if(!IsPlayerConnected(i)) continue;
			GetPlayerName(i,name,30);
			if(strfind(name,tmp,true) != -1)
			{
				if(id >= 4) {
					id++;
					continue;
				}
				format(name,sizeof(name),"%s [%d]",name,i);
				SendClientMessage(playerid,COLOR_WHITE,name);
				id++;
			}
		}
		if(id == -1) return SendClientMessage(playerid,COLOR_WHITE,"Не найдено");
		else if(id >= 4) {
			format(name,sizeof(name),"Показано 5 найденных из %d", id-2);
			SendClientMessage(playerid, COLOR_GRAD1, name);
		}
	}
	else
	{
		id = strval(tmp);
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid,COLOR_WHITE,"Не верный ID");
		GetPlayerName(id,name,24);
		format(name,sizeof(name),"%s [%d]",name,id);
		SendClientMessage(playerid,COLOR_WHITE,name);
	}
	return 1;
}
CMD:mark(playerid, params[])
{
