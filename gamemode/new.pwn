stock GzCheck()
{
	for(new i =1;i<=TOTALGZ;i++)
	{
		if(GZSafeTime[i] > 0)
		{
			GZSafeTime[i]--;
			foreach (new x : Player)
			{
				if(Player[x][pMember] == GZInfo[i][gNapad] || Player[x][pMember] == GZInfo[i][gFrak])
				{
					if(GetPVarInt(x,"GzOn") == 0)
					{
						new BANDAA[128],BANDAB[128];
						PlayerTextDrawShow(x, BandaCapt1a[x]);
						PlayerTextDrawShow(x, BandaCapt2a[x]);
						PlayerTextDrawShow(x, ScoreCapt1a[x]);
						PlayerTextDrawShow(x, ScoreCapt2a[x]);
						PlayerTextDrawShow(x, CaptTime[x]);
						PlayerTextDrawShow(x, Textdraw4[x]);
						
						format(BANDAA, 128, "~y~%s", GetGangName(GZInfo[i][gNapad]));
						PlayerTextDrawSetString(x,BandaCapt1a[x], BANDAA);
						
						format(BANDAB, 128, "~y~%s", GetGangName(GZInfo[i][gFrak]));
						PlayerTextDrawSetString(x,BandaCapt2a[x], BANDAB);
						
