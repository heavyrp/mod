						{
							if(health <= 25 && PlayerInfo[giveplayerid][pUseKrisha] != 0)
							{
								if(PlayerInfo[giveplayerid][pDolg] < 50) return SendClientMessage(playerid,COLOR_GREY,"Он/Она еще ни чего не заработал(а)!");
								SendClientMessage(playerid,COLOR_GREEN,"Вы избили должника до 25 хп, долг был выбит");
								GangStore[bdnomer][gMoney] += PlayerInfo[giveplayerid][pDolg];
								PlayerInfo[giveplayerid][pDolg] = 0;
								PlayerInfo[giveplayerid][pUseKrisha] = 0;
								return true;
							}
						}
					}
				}
			}
		}
	}
	else if(newkeys == KEY_SUBMISSION)
	{
		CallLocalFunction("OnPlayerCommandText", "is", playerid, "/engggg");
	}
	if(newkeys == 1024)
	{
		if(!IsPlayerInAnyVehicle(playerid))
		{
			if(IsABankomat(playerid))
			{
				if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) { return true; }
				ApplyAnimation(playerid,"CRIB","CRIB_Use_Switch",4.0,0,0,0,0,0);
				ShowPlayerDialog(playerid, 8900, DIALOG_STYLE_LIST, "Терминал приёма платежей", "[1] > Снять наличные\n[2] > Баланс банковского счёта\n[3] > Пополнение домашнего счёта", "Далее >>", "Выход");
				return true;
			}
		}
	}
	if(newkeys == 16)
	{
		if(IsAFoods(playerid)) return ClearAnimations(playerid, true);
	}
	if((newkeys & KEY_SECONDARY_ATTACK && GetPVarInt(playerid,"Animation") == 1))
	{
		ClearAnimations(playerid);
		SetPVarInt(playerid,"Animation",0);
		TextDrawHideForPlayer(playerid, AnimDraw[playerid]);
	}
	if(newkeys & KEY_SPRINT && newkeys & KEY_JUMP)
	{
		if(IsAGang(playerid))
		{
			if(usemats[playerid] == 1)
			{
				RemovePlayerAttachedObject(playerid,1);
				SendClientMessage(playerid,COLOR_RED,"Вы уронили Ящик!");
				PlayerInfo[playerid][pMats] -= 250;
				usemats[playerid] = 0;
				return true;
			}
		}
	}
	if(newkeys == KEY_FIRE)
	{
		if(IsAGang(playerid))
		{
			if(usemats[playerid] == 1)
			{
				RemovePlayerAttachedObject(playerid,1);
				SendClientMessage(playerid,COLOR_RED,"Вы уронили Ящик!");
				PlayerInfo[playerid][pMats] -= 250;
				usemats[playerid] = 0;
				return true;
			}
		}
	}
	if(newkeys & KEY_SECONDARY_ATTACK  || (newkeys & KEY_SUBMISSION  && newkeys & KEY_SECONDARY_ATTACK ))
	{
		if(IsAGang(playerid))
		{
			if(usemats[playerid] == 1)
			{
				RemovePlayerAttachedObject(playerid,1);
				SendClientMessage(playerid,COLOR_RED,"Вы уронили Ящик!");
				PlayerInfo[playerid][pMats] -= 250;
				usemats[playerid] = 0;
				return true;
			}
		}
	}
	return true;
}
