	format(stringer, BIG_SIZE, "{FFFFFF}Название бизнеса: {269BD8}%s\n\n{FFFFFF}Владелец: {269BD8}%s\n{FFFFFF}Гос. цена: {269BD8}$%i\n{FFFFFF}Состояние: %s\n\n{FFFFFF}- За оплату помещения: {269BD8}$%i\n{FFFFFF}Бюджет бизнеса: {269BD8}$%i",BizInfo[bizid][bName],BizInfo[bizid][bOwner],BizInfo[bizid][bPrice],status,BizInfo[bizid][bNalog],BizInfo[bizid][bBank]);
	SPD(playerid,0000,DIALOG_STYLE_MSGBOX,"Бизнес",stringer,"Выход","");
}
