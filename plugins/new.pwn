	if(PlayerInfo[params[0]][pAdmin] >= 1 && PlayerInfo[playerid][pAdmin] < 7)
	{
		format(strings, 128, "Администратор %s Забанен. Обратитесь к гл.администратору", PlayerInfo[playerid][pNames]);
		SendAdminMessage(COLOR_LIGHTRED,strings);
		format(strings,sizeof(strings),"\t\t\t{ff5000}Ваш аккаунт заблокирован.\n{ff8800}Заблокирован(а): {ffffff}%s\n{ff8800}Заблокировал(а): {ffffff}система\n{ff8800}Срок бана: {ffffff}навсегда{ff8800}\nПричина бана: {ffffff}нарушение прав",PlayerInfo[playerid][pNames]);
		ShowPlayerDialog(playerid,228,DIALOG_STYLE_MSGBOX,"Блокировка аккаунта",strings,"Да","");
