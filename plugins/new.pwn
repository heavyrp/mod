}
CMD:fare(playerid, params[])
{
	new Veh = GetPlayerVehicleID(playerid);
	if(PlayerInfo[playerid][pJob] == 4)
	{
		if(TransportDuty[playerid] > 0)
		{
			if(TransportDuty[playerid] == 1)
			{
				TaxiDrivers -= 1;
			}
			new moneyyys = 32 + random(2);
			DestroyDynamic3DTextLabel(taxi3d[Veh]);
			TransportDuty[playerid] = 0;
			format(strings, sizeof(strings), "Вы закончили рабочий день, ваш бонус: %d", moneyyys);
			SendClientMessage(playerid, 0x6495EDFF, strings);
			PlayerInfo[playerid][pCash] +=TransportMoney[playerid];
			TransportValue[playerid] = 0; TransportMoney[playerid] = 0;
			return true;
		}
		if(Veh >= taxicar[0] && Veh <= taxicar[1] || Veh >= marcar[0] && Veh <= marcar[9])
		{
			if(GetPlayerState(playerid) == 2)
			{
				if(sscanf(params,"d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, " Введите: /fare [тариф]");
				if(params[0] < 1 || params[0] > 1000) { SendClientMessage(playerid, COLOR_WHITE, "Не меньше 1 и не больше $1000"); return true; }
				TaxiDrivers += 1;
				TransportDuty[playerid] = 1;
				TransportValue[playerid] = params[0];

				if(Veh >= taxicar[0] && Veh <= taxicar[1])
				{
					new stringer[128];
					format(stringer, sizeof(stringer), "{3399ff}Такси\n{ffff00}Тариф: %d$", TransportValue[playerid]);
					taxi3d[Veh] = CreateDynamic3DTextLabel(stringer, -1, 9999.0, 9999.0, 9999.0, 30.0,INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 1, 0);
				}
			}
			else SendClientMessage(playerid, COLOR_WHITE, "{F36223}[!]{ADAEAD}Вы не таксист или вы не в машине.");
		}
		else  SendClientMessage(playerid, COLOR_WHITE, "{F36223}[!]{ADAEAD}Вы не начали рабочий день");
	}
	else SendClientMessage(playerid, COLOR_WHITE, "{F36223}[!]{ADAEAD}Вы не таксист или вы не в машине.");
	return true;
}
