CMD:capture(playerid, params[])
{
	if(nocapture == 1) return SendClientMessage(playerid, -1, "Запрет на капт включен администрацией");
	if(IsAGang(playerid))
	{
		new h; gettime(h);
		if(h < 9 || h > 21 ) return SendClientMessage(playerid, COLOR_GREY, "Использовать команду можно только с 9:00 до 21:00");
		if(Player[playerid][pRank] < 8) return SendClientMessage(playerid,COLOR_GREY,"Функция доступная с 8 ранга и выше!");
		new faction = Player[playerid][pMember];
		if(!IsInAllowed(playerid)) return SendClientMessage(playerid, COLOR_GREY, "Ваша банда уже учавствует в захвате зон!");
		for(new i = 1;i<=TOTALGZ;i++)
		{
			if(PlayerToKvadrat(playerid,GZInfo[i][gCoords][0], GZInfo[i][gCoords][1],GZInfo[i][gCoords][2],GZInfo[i][gCoords][3]) && ZoneOnBattle[i] == 0)
			{
				if(IsPlayerInAnyVehicle(playerid)) return  SendClientMessage(playerid, COLOR_GREY, "Нельзя использовать в машине");
				{
					if(GZInfo[i][gFrak] == faction) return SendClientMessage(playerid, COLOR_GREY, "Вы не можете захватывать зону вашей банды!");
					if(IsCapture == 1) return SendClientMessage(playerid, COLOR_GREY, "Уже происходит захват одной из зон. Дождитесь окончания!");
					if(IsATerra(playerid)) return SendClientMessage(playerid,COLOR_GREY,"Нельзя захватить эту территорию");
					if(IsPlayerInBandOnline(faction) < 2) return SendClientMessage(playerid,COLOR_GREY, "В вашей банде мало игроков онлайн!");
					if(IsPlayerInBandOnline(GZInfo[i][gFrak]) < 2) return format(stringer1,128,"У банды \"%s\" мало игроков!", GetGangName(GZInfo[i][gFrak])), SendClientMessage(playerid,0x6495EDFF, stringer1);
					new zahvatka[128];
					format(zahvatka, 127,"[F] Ваша зона атакуется бандой %s, у вас есть 5 минут, чтобы отбиться", GetPlayerFracName(playerid));
					SendFamilyMessage(GZInfo[i][gFrak],COLOR_LIGHTRED,zahvatka);
					format(stringer1, 127, "[F] Если по истечению времени вас не будет на зоне, она перейдет к %s",GetPlayerFracName(playerid));
					SendFamilyMessage(GZInfo[i][gFrak],COLOR_LIGHTRED,stringer1);
					format(stringer1, sizeof(stringer1), "Внимание! Боец %s из банды %s атакует банду %s!", Player[playerid][pNames], GetPlayerFracName(playerid), GetGangName(GZInfo[i][gFrak]));
					ABroadCast(COLOR_GREY,stringer1,1);
					//------------------------------------
					new zahvat[128];
					format(zahvat,128,"[F] %s начал захват зоны банды %s.",Player[playerid][pNames],GetGangName(GZInfo[i][gFrak]));
					SendFamilyMessage(faction,COLOR_LIGHTRED,zahvat);
					GangZoneFlashForAll(GZInfo[i][id],GetGZColorF(faction));
					format(zahvat, sizeof(zahvat), "[A-INFO] %s начал захват зоны банды %s.",Player[playerid][pNames],GetGangName(GZInfo[i][gFrak]));
					SendAdminMessage(COLOR_GREY,zahvat);
					GZSafeTime[i] = 420;
					FrakCD[faction]  = 3600;
					GZInfo[i][gNapad] = faction;
					ZoneOnBattle[i] = 1;
					IsCapture = 1;
					GangInfo[faction][capture] = 1;
					GangInfo[GZInfo[i][gFrak]][capture] = 1;
					GangInfo[GZInfo[i][gFrak]][captureid] = faction;
					GangInfo[faction][captureid] = faction;
					GangInfo[faction][gangnumber] = 1;
					GangInfo[GZInfo[i][gFrak]][gangnumber] = 0;
					Timer[7] = gettime()+1;
				}
			}
		}
	}
	else SendClientMessage(playerid, COLOR_GREY, YouCanNot);
	return true;
}
