stock TipsterPlayer(playerid)
{
	new vehicle = GetSpyVehicle(playerid);
	if(tipsterl2[playerid] == true)
	{
		if(!IsVehicleStreamedIn(vehicle, playerid) && tipsterfail[playerid] == false)
		{
			tipsterl[playerid] = false;
			tipsterfail[playerid] = true;
			SendClientMessage(playerid, 0x00b953ff ,"Оборудование вне зоны досягаемости, связь прервана...");
		}
		else if(GetDistanceBetweenPlayers(playerid, tipsterlisten) > 200 && tipsterfail[playerid] == false && tipsterl[playerid] == true)
		{
			tipsterl[playerid] = false;
			SendClientMessage(playerid, 0x00b953ff ,"Жучок вне зоны досягаемости, прослушивание окончено");
		}
	}
	if(!tipsterl[playerid] && tipsterl2[playerid] == true)
	{
		if(IsVehicleStreamedIn(vehicle, playerid) && tipsterfail[playerid] == true)
		{
			SendClientMessage(playerid, 0x00b953ff ,"Связь восстановлена"), tipsterfail[playerid] = false;
			if(GetDistanceBetweenPlayers(playerid, tipsterlisten) < 200) tipsterl[playerid] = true;
		}
		else if(GetDistanceBetweenPlayers(playerid, tipsterlisten) < 200 && tipsterfail[playerid] == false)
		{
			tipsterl[playerid] = true;
			SendClientMessage(playerid, 0x00b953ff ,"Прослушивание возобновлено");
		}
	}
	if(!IsPlayerConnected(tipsterlisten) || tipster == false)
	{
		SetPVarInt(playerid, "Tipster", 0);
		tipsterr[playerid] = false;
		tipsterOn[playerid] = false;
		tipsterl[playerid] = false;
		tipsterl2[playerid] = false;
		tipster = false;
		tipsterlisten = -1;
		tipsters[playerid] = -1;
		SendFamilyMessage(2, 0x00b953ff, "Жучок деактивирован");
	}
	return true;
}
stock GetSpyVehicle(playerid)
{
	for(new i = 0; i < MAX_VEHICLES; i++)
	{
		if(IsVehicleStreamedIn(i, playerid))
		{
			if(i >= fbicar[0] && i <= fbicar[1]) return i;
		}
	}
	return -1;
}
