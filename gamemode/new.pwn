							}
							Player[params[0]][pRank] = rankdemote;
							format(stringer1, sizeof(stringer1), "Агент FBI %s понизил вас до %d ранга", Player[playerid][pNames], rankdemote);
							SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
							format(stringer1, sizeof(stringer1), "Вы понизили %s до %d ранга.", Player[params[0]][pNames],rankdemote);
							SendClientMessage(playerid, 0x6ab1ffaa, stringer1);

						}

					}
				}
			}
		}
	}
	return true;
}
CMD:gotomark(playerid, params[])
