	CharPrice[playerid] = 0;
	SelectCharID[playerid] = 0;
	SelectChar[playerid] = 0;
	PicCP[playerid] = 0;
	repairdvig[playerid] = 0;
	takephone[playerid] = 0;
	gag[playerid] = 0;
	dostup[playerid] = 0;
	PicCP[playerid] = 0;
	enterbiz[playerid] = 0;
	Player[playerid][pDrugtime] = 0;
	Player[playerid][pGunfbiTime] = 0;
	forma[playerid] = 0;
	proverkaforma[playerid] = 0;
	keys[playerid] = 0;
	SetPlayerSkills(playerid);
