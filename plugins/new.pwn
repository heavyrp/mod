publics: OnPlayerSelectOffban(playerid,tmp[],reason[])
{
	new year, month, hour1, minute1, second1,day,query[144];
	getdate(year, month, day);
	gettime(hour1, minute1, second1);
	new curdate = getdate(year,month,day),rows, fields,stringer[144];
	cache_get_data(rows, fields);
	if(!rows) return SendClientMessage(playerid,COLOR_GREY,"Файл-аккаунта не найден");
	format(query, sizeof(query), "UPDATE `accounts` SET `pLocked2` = '1',`pBanData1` = '%d', `pBanTime1` = '%d' WHERE `Name` = '%s'",curdate+7,curdate, tmp);
	mysql_function_query(connects, query, false, "", "");
	if (minute1 < 10)
	{
		format(stringer, sizeof(stringer), "OffBan[забанил: %s][забанен: %s][причина: %s][%d/%d/%d  %d:0%d]",PlayerInfo[playerid][pNames],tmp,reason,day,month,year,hour1,minute1);
		SendAdminMessage(COLOR_LIGHTRED,stringer);
	}
	else
	{
		format(stringer, sizeof(stringer), "OffBan[забанил: %s][забанен: %s][причина: %s][%d/%d/%d  %d:%d]",PlayerInfo[playerid][pNames],tmp,reason,day,month,year,hour1,minute1);
