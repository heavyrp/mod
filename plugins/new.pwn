CMD:nalog(playerid, params[])
{
	if(PlayerInfo[playerid][pLeader] == 7)
	{
		if(sscanf(params,"d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, " Введите: /nalog [кол-во]");
		if(params[0] < 1000 || params[0] > 15000) return SendClientMessage(playerid, COLOR_WHITE, " Налог не можеть быть меньше 1000 и больше 15000");
		if(PlayerToPoint(50.0,playerid,360.8623,173.5765,1009.1094))
		{
			new stringer[150];
			nalog = params[0];
			SendClientMessage(playerid, COLOR_WHITE, "Вы изменили налог штата.");
			SendClientMessageToAll(COLOR_WHITE, " ===== [Срочные новости] ===== ");
			format(stringer, sizeof(stringer), "Призидент штата поменял налог государства. Теперь всемирный налог равен {FFFFFF}%d {6495ED}вирт.",params[0]);
