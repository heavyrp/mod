	{
		if(!IsAPlane(v) && !IsABoat(v) && !IsANoSpeed(v)) Fuell[v] = 0,EngineState[playerid] = false;
	}
	if(!IsAPlane(v) && !IsABoat(v) && !IsANoSpeed(v))
	{
