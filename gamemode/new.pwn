CMD:sellfuel(playerid, params[])
{
	if(Player[playerid][pPsbiskey] == 255) return SendClientMessage(playerid,COLOR_WHITE,"У вас нету заправки.");
	for(new b, z = sizeof(SBizzInfo); b<z; b++)
	{
		if(!PlayerToPoint(10.0, playerid, SBizzInfo[b][sEntranceX], SBizzInfo[b][sEntranceY], SBizzInfo[b][sEntranceZ])) return true;
		if(Player[playerid][pPsbiskey] != b) return SendClientMessage(playerid,COLOR_WHITE,"Вы не владелец этой заправки.");
		new nalogs = SBizzInfo[b][sBuyPrice]/10;
		Player[playerid][pBank] += (SBizzInfo[b][sBuyPrice]-nalogs);
		Player[playerid][pPsbiskey] = 255;
		SBizzInfo[b][sOwned] = 0;
		SBizzInfo[b][sLocked] = 1;
		SBizzInfo[b][sbMafia] = 0;
		strmid(SBizzInfo[b][sOwner], "No-one", 0, strlen("No-one"), 255);
		format(stringer1, MAX_STR, "UPDATE `"TABLE_SBIZZ"` SET `sOwner` = 'No-one' WHERE sID = %i",b);
		//mysql_tquery(connects, stringer1);
		mysql_function_query(connects, stringer1, false, "", "");
		SendClientMessage(playerid, COLOR_GREY, "Вы успешно продали свою заправку.");
		SaveSBizz(b);
		UpdateSBizz();
		return true;
	}
	return true;
}
