				format(stringer1,90,"[F] Банда %s выйграла, вы проиграли захват зоны",GetGangName(zx));
				SendFamilyMessage(GZInfo[i][gNapad],COLOR_LIGHTRED, stringer1);
				ZoneOnBattle[zl] =0;
				CaptureEnd(GZInfo[zl][gNapad], GZInfo[zl][gFrak]);
