stock GzCheck()
{
	if(capt == 1 || capt == 10)//capture[12] == 1 || capture[13] == 1 || capture[15] == 1 || capture[17] == 1 || capture[18] == 1)
	{
		if(WarZoneID == 255) return true;
		WarTimeSec--;
		if(WarTimeSec > 1 && WarTimeMin >= 0 || WarTimeMin > 0)
		{
			if(WarTimeSec == 0 && WarTimeMin != 0) { WarTimeSec = 60; WarTimeMin--; }
			new stringtime[32];
			if(WarTimeSec > 9) format(stringtime, sizeof(stringtime),"  %d:%d",WarTimeMin,WarTimeSec);
			else format(stringtime, sizeof(stringtime),"  %d:0%d",WarTimeMin,WarTimeSec);
			TextDrawSetString(vremya, stringtime);
		}
		else
		{
			foreach(Player, z)
			{
				if(PlayerInfo[z][pMember] == 12 || PlayerInfo[z][pMember] == 13 || PlayerInfo[z][pMember] == 15 || PlayerInfo[z][pMember] == 17 || PlayerInfo[z][pMember] == 18)
				{
					TextDrawHideForPlayer(z, fon);
					TextDrawHideForPlayer(z, onecommand);
					TextDrawHideForPlayer(z, twocommand);
					TextDrawHideForPlayer(z, kolone);
					TextDrawHideForPlayer(z, koltwo);
					TextDrawHideForPlayer(z, timetext);
					TextDrawHideForPlayer(z, vremya);
				}
			}
			GangZoneStopFlashForAll(WarZoneID);
			GangZoneHideForAll(WarZoneID);
			if(CommandKill[1] > CommandKill[0])
			{
				SendFamilyMessage(captureteam[1],0x72e600FF,"[F] Вам удалось отстоять свою территорию");
				SendFamilyMessage(captureteam[0],COLOR_RED,"[F] Вам не удалось завоевать территорию");
				switch(GZInfo[WarZoneID][gFrakVlad])
				{
				case 12: GangZoneShowForAll(GZInfo[WarZoneID][gID],0xCC00FFAA);
				case 18: GangZoneShowForAll(GZInfo[WarZoneID][gID],0x6666FFAA);
				case 13: GangZoneShowForAll(GZInfo[WarZoneID][gID],0xffff00AA);
				case 15: GangZoneShowForAll(GZInfo[WarZoneID][gID],0x009900AA);
				case 17: GangZoneShowForAll(GZInfo[WarZoneID][gID],0x00CCFFAA);
				default: GangZoneShowForAll(GZInfo[WarZoneID][gID],0xC0C0C0AA);
				}
				FrakCD[GZInfo[WarZoneID][gNapad]] = 0;
				FrakCD[GZInfo[WarZoneID][gFrakVlad]] = 0;
				ScoreZahvat[GZInfo[WarZoneID][gNapad]] = 0;
				GZInfo[WarZoneID][gNapad] = 0;
			}
			else
			{
				SendFamilyMessage(captureteam[1],COLOR_RED,"[F] Вы потеряли контроль над одной из своих территорий");
				SendFamilyMessage(captureteam[0],0x72e600FF,"[F] Вы приобрели контроль над новой территорией");
				switch(GZInfo[WarZoneID][gNapad])
				{
				case 12: GangZoneShowForAll(GZInfo[WarZoneID][gID],0xCC00FFAA);
				case 18: GangZoneShowForAll(GZInfo[WarZoneID][gID],0x6666FFAA);
				case 13: GangZoneShowForAll(GZInfo[WarZoneID][gID],0xffff00AA);
				case 15: GangZoneShowForAll(GZInfo[WarZoneID][gID],0x009900AA);
				case 17: GangZoneShowForAll(GZInfo[WarZoneID][gID],0x00CCFFAA);
				default: GangZoneShowForAll(GZInfo[WarZoneID][gID],0xC0C0C0AA);
				}
				FrakCD[GZInfo[WarZoneID][gNapad]] = 0;
				FrakCD[GZInfo[WarZoneID][gFrakVlad]] = 0;
				ScoreZahvat[GZInfo[WarZoneID][gFrakVlad]] = 0;
				ScoreZahvat[GZInfo[WarZoneID][gNapad]] = 0;
				GZInfo[WarZoneID][gFrakVlad] = GZInfo[WarZoneID][gNapad];
				GZInfo[WarZoneID][gNapad] = 0;
			}
			captureteam[0] = 0;
			captureteam[1] = 0;
			WarTimeSec = 0;
			if(capt == 1) capt = 2;
			else if(capt == 10) capt = 2;
			WarZoneID = 255;
			SaveGZ();
		}
	}
	return true;
}
