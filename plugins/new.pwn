stock StartReconForPlayer(playerid, specid)
{
	if(PlayerInfo[playerid][pAdmin] < 1 || !IsPlayerConnected(specid) || gPlayerSpawned[specid] == 0) return SendClientMessage(playerid,COLOR_GREY,"Игрок оффлайн / Не залогинен!");
	if(GetPVarInt(playerid, "spectate_admin") == 0)
	{
		new Float:Pos[3];
		GetPlayerPos(playerid, Pos[0], Pos[1], Pos[2]);
		SetPVarFloat(playerid, "PosSpecX", Pos[0]);
		SetPVarFloat(playerid, "PosSpecY", Pos[1]);
		SetPVarFloat(playerid, "PosSpecZ", Pos[2]);
		SetPVarInt(playerid, "SpecInt", GetPlayerInterior(playerid));
		SetPVarInt(playerid, "SpecVT", GetPlayerVirtualWorld(playerid));
	}
	ShowReconMenuForPlayer(playerid);
	ShowReconPlayerInfoForPlayer(playerid);
	TogglePlayerSpectating(playerid, 1);
	SetPVarInt(playerid, "SpecID", specid);
	SetPVarInt(playerid, "spectate_admin", 1);
	SetPVarInt(specid, "spectate_player", 1);
	SetPlayerInterior(playerid, GetPlayerInterior(specid));
	SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(specid));
	if(!IsPlayerInAnyVehicle(specid)) return PlayerSpectatePlayer(playerid, specid);
	else return PlayerSpectateVehicle(playerid, GetPlayerVehicleID(specid));
}
stock StopRecon(playerid, reason[])
{
	if(PlayerInfo[playerid][pAdmin] < 1 || GetPVarInt(playerid, "spectate_admin") == 0) return true;
	HideReconMenuForPlayer(playerid);
	HideReconPlayerInfoForPlayer(playerid);
	gPlayerSpawned[playerid] = 0;
	TogglePlayerSpectating(playerid, 0);
	DeletePVar(GetPVarInt(playerid, "SpecID"), "spectate_player");
	DeletePVar(playerid, "SpecID");
	GameTextForPlayer(playerid, reason, 1500, 4);
	return true;
}
