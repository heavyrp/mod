				{
					SetPlayerInterior(params[0],inter);
					Player[params[0]][pInt] = inter;
					Player[params[0]][pLocal] = Player[playerid][pLocal];
					SetPlayerVirtualWorld(params[0],worldd);
				}
				if(Player[playerid][pInt] == 0)
				{
					SetPlayerInterior(params[0],Player[playerid][pInt]);
					SetPlayerInterior(params[0],inter);
					SetPlayerVirtualWorld(params[0],worldd);
				}
				if(plocz > 930.0 && Player[playerid][pInt] == 0) //the highest land point in sa = 526.8
				{
					SetPlayerInterior(params[0],inter);
					Player[params[0]][pInt] = inter;
				}
				if (GetPlayerState(params[0]) == 2)
				{
					SetPlayerInterior(params[0],inter);
					new tmpcar = GetPlayerVehicleID(params[0]);
					SetPlayerVirtualWorld(params[0],worldd);
					SetVehicleEE(tmpcar, plocx, plocy+4, plocz);
