    {
        if(PlayerInfo[playerid][pLoadName] != PlayerInfo[playerid][pNames]) return true;
        new pass[16];
        new level = cache_get_row_int(0, 0);
        cache_get_row(0, 1, pass);
		PlayerInfo[playerid][pDostup] = pass;
	    if(!strlen(PlayerInfo[playerid][pDostup]))
		{
            mysql_format(connects, strings, 128, "UPDATE `admins` SET Dostup = '%s' WHERE name = '%s'", Dostup[playerid], PlayerInfo[playerid][pNames]);
            mysql_query(strings, -1, -1, connects);
            PlayerInfo[playerid][pAdmin] = level;
		    dostup[playerid] = 1;
		    if(PlayerInfo[playerid][pAdmin] > 0 && PlayerInfo[playerid][pAdmin] < 6)
			{
				format(strings, sizeof(strings),"Вы авторизировались как модератор %d уровня", PlayerInfo[playerid][pAdmin]);
				SendClientMessage(playerid, 0xEAC700FF, strings);
		 	}
            if(PlayerInfo[playerid][pAdmin] > 5)
			{
			    format(strings, sizeof(strings),"Вы авторизировались как администратор %d уровня", PlayerInfo[playerid][pAdmin]);
				SendClientMessage(playerid, 0xEAC700FF, strings);
			}
		    format(strings, 50, "%s[%d] авторизовался на сервере.", PlayerInfo[playerid][pNames], playerid);
		    SendAdminMessage(0xEAC700FF, strings);
            format(strings, 57, "- IP: %s | Уровень администрирования: %d", PlayerInfo[playerid][pGetIp], PlayerInfo[playerid][pAdmin]);
		    SendAdminMessage(0xEAC700FF, strings);
            TogglePlayerControllable(playerid, true);
		}
		else
		{
		    if(!strcmp(Dostup[playerid], PlayerInfo[playerid][pDostup], true))
            {
                PlayerInfo[playerid][pAdmin] = level;
		        dostup[playerid] = 1;
		        if(PlayerInfo[playerid][pAdmin] > 0 && PlayerInfo[playerid][pAdmin] < 6)
				{
				    format(strings, sizeof(strings),"Вы авторизировались как модератор %d уровня", PlayerInfo[playerid][pAdmin]);
					SendClientMessage(playerid, 0xEAC700FF, strings);
				}
                if(PlayerInfo[playerid][pAdmin] > 5)
				{
				    format(strings, sizeof(strings),"Вы авторизировались как администратор %d уровня", PlayerInfo[playerid][pAdmin]);
					SendClientMessage(playerid, 0xEAC700FF, strings);
				}
		        format(strings, 50, "%s[%d] авторизовался на сервере.", PlayerInfo[playerid][pNames], playerid);
		        SendAdminMessage(0xEAC700FF, strings);
                format(strings, 57, "- IP: %s | Уровень администрирования: %d", PlayerInfo[playerid][pGetIp], PlayerInfo[playerid][pAdmin]);
		        SendAdminMessage(0xEAC700FF, strings);
                TogglePlayerControllable(playerid, true);
			}
			else
			{
		        if(GetPVarInt(playerid, "wrongPass") == 2)
		        {
			        KickEx(playerid);
			        return true;
		        }
                SetPVarInt(playerid, "wrongPass", GetPVarInt(playerid, "wrongPass")+1);
                format(strings, 256, "{D95A41}Внимание! Вы ввели неверный пароль!\nУ вас осталось (%d) попытки\n\n{a9c4e4}Введите пароль\n\nПароль должен состоять из латинских букв и цифр\n\tразмером от 6 до 15 символов", 3-GetPVarInt(playerid, "wrongPass"));
                ShowPlayerDialog(playerid,2934,DIALOG_STYLE_PASSWORD,"Админ авторизация", strings, "Вход", "Отмена");
                PlayerPlaySound(playerid, 1150, 0.0, 0.0, 0.0);
			}
		}
	}
	else
	{
	    if(PlayerInfo[playerid][pLoadName] != PlayerInfo[playerid][pNames]) return true;
	    SCM(playerid, COLOR_WHITE, "Ошибка: Вы не модератор.");
	}
	return true;
}
