stock SaveSBizz(houseid)
{
	new update[600],stringer[64];
	update = "UPDATE `sbizz` SET ";
	acc_int_strcat(update, sizeof(update), "sOwned", SBizzInfo[houseid][sOwned]);
	acc_str_strcat(update, sizeof(update), "sMessage", SBizzInfo[houseid][sMessage]);
	acc_int_strcat(update, sizeof(update), "sBuyPrice", SBizzInfo[houseid][sBuyPrice]);
	acc_int_strcat(update, sizeof(update), "sLocked", SBizzInfo[houseid][sLocked]);
	acc_int_strcat(update, sizeof(update), "sPrice", SBizzInfo[houseid][sPrice]);
	acc_int_strcat(update, sizeof(update), "sBenz", SBizzInfo[houseid][sBenz]);
	acc_int_strcat(update, sizeof(update), "sPriceBenz", SBizzInfo[houseid][sPriceBenz]);
	acc_int_strcat(update, sizeof(update), "sTill", SBizzInfo[houseid][sTill]);
	strdel(update, strlen(update)-1, strlen(update));
	format(stringer,sizeof(stringer)," WHERE `sID` = '%d'",houseid);
	strcat(update, stringer);
	mysql_function_query(connects, update, false, "", "");
}
stock SaveAll()
{
	for(new sb = 0; sb < sizeof(SBizzInfo); sb++) SaveSBizz(sb);
	for(new a = 0; a < sizeof(AvtoInfo); a++) SaveAvto(a);
	for(new b = 0; b < sizeof(BizzInfo); b++) SaveBizz(b);
	return true;
}
stock SaveHousee()
{
	for(new h = 0; h < sizeof(HouseInfo); h++) SaveHouse(h);
	return true;
}
stock SellBizz(playerid)
{
	new str[64];
	if(PlayerInfo[playerid][pPbiskey] != 255)
	{
		new bouse = PlayerInfo[playerid][pPbiskey];
		BizzInfo[bouse][bLocked] = 1;
		BizzInfo[bouse][bOwned] = 0;
		BizzInfo[bouse][bProducts] = 0;
		strmid(BizzInfo[bouse][bOwner], "The State", 0, strlen("The State"), 255);
		strmid(BizzInfo[bouse][bExtortion], "No-one", 0, strlen("No-one"), 255);
		SendClientMessage(playerid, COLOR_YELLOW2, "::: Бизнес успешно продан :::");
		format(strings, sizeof(strings), "- Прибыль с продажи бизнеса: %d вирт", BizzInfo[bouse][bTill]);
		SendClientMessage(playerid, COLOR_WHITE, strings);
		format(strings, sizeof(strings), "- Новый счёт в банке: %d вирт", PlayerInfo[playerid][pBank]);
		SendClientMessage(playerid, COLOR_WHITE, strings);
		SendClientMessage(playerid, COLOR_YELLOW2, "::: Бизнес успешно продан :::");
		PlayerInfo[playerid][pBank] +=BizzInfo[bouse][bTill] / 2;
		PlayerInfo[playerid][pBank] +=BizzInfo[bouse][b2Till];
		BizzInfo[bouse][bTill] = 0;
		BizzInfo[bouse][b2Till] = 0;
		format(str, sizeof(str), "{00FF32}Цена:{FEFEFE} %d\n{F5FF00}Бизнес продаётся",BizzInfo[bouse][bBuyPrice]);
		Update3DTextLabelText(BizzInfo[bouse][bLabel], COLOR_YELLOW, str);
		SaveBizz(bouse);
		PlayerInfo[playerid][pPbiskey] = 255;
	}
	return true;
}
