SellBusiness(playerid)
{
	if(IsPlayerInRangeOfPoint( playerid, 10.0, BizInfo[Player[playerid][pPbiskey]][ EnterX ], BizInfo[Player[playerid][pPbiskey]][ EnterY ], BizInfo[Player[playerid][pPbiskey]][ EnterZ ] ) )
	{
		strmid(BizInfo[Player[playerid][pPbiskey]][bOwner], "None", 0, strlen("None"), 255);
		BizInfo[Player[playerid][pPbiskey]][bLock] = 0;
		BizInfo[Player[playerid][pPbiskey]][bPribil] = 0;
		new update[100],stringerg[64];
		update = "UPDATE `"TABLE_BUSSINESS"` SET ";
		acc_str_strcat(update, sizeof(update), "bOwner", "None");
		acc_int_strcat(update, sizeof(update), "bLock", 0);
		acc_int_strcat(update, sizeof(update), "bPribil", 0);
		strdel(update, strlen(update)-1, strlen(update));
		format(stringerg,sizeof(stringerg)," WHERE `bID` = '%d'",Player[playerid][pPbiskey]);
		strcat(update, stringerg);
		//mysql_tquery(connects, update);
		mysql_function_query(connects, update, false, "", "");
		GiveMoney(playerid,BizInfo[Player[playerid][pPbiskey]][bPrice]);
		UpdateBizInfo (Player[playerid][pPbiskey]);
		SendMes(playerid,COLOR_WHITE,"Вы продали свой бизнес за {FFFF00}%d",BizInfo[Player[playerid][pPbiskey]][bPrice]);
		Player[playerid][pPbiskey] = 255;
	}
	else return SendClientMessage(playerid,COLOR_GREY,"{d83927}Ошибка {FFFFFF}|{e3e3e3} Надо находиться рядом с бизнесом");
	return true;
}

CheckBusiness(bizid)
{
	new query[400];
	new playeridd = GetPlayerID(BizInfo[bizid][bOwner]);
	BizInfo[bizid][bBank] += BizInfo[bizid][bPribil];
	BizInfo[bizid][bPribil] = 0;
	if(!strcmp(BizInfo[bizid][bOwner], "None",true)) {}
	else
	{
		if(BizInfo[bizid][bBank] < BizInfo[bizid][bNalog])
		{
			if(!IsPlayerConnected(playeridd))
			{
				format(query, MAX_SQL, "UPDATE `accounts` SET `pPbiskey` = '255' WHERE `Name` = '%s'", BizInfo[bizid][bOwner]);
				//mysql_tquery(connects, query);
				mysql_function_query(connects, query, false, "", "");
			}
			else
			{
				SendClientMessage(playeridd, -1, "Ваш бизнес продан за неуплату помещения.");
				Player[playeridd][pPbiskey] = 255;
			}
			BizInfo[bizid][bPribil] = 0;
			BizInfo[bizid][bLock] = 0;
			BizInfo[bizid][bBank] = 0;
			strmid(BizInfo[bizid][bOwner], "None", 0, strlen("None"), 255);
			new stringer13[150];
			format(stringer13,sizeof(stringer13),"UPDATE `"TABLE_BUSSINESS"` SET bPribil=0,bLock=0,bBank=0,bOwner='None' WHERE bID = %d",bizid);
			//mysql_tquery(connects,stringer13);
			mysql_function_query(connects, stringer13, false, "", "");
			UpdateBizInfo(bizid);
		}
		else BizInfo[bizid][bBank] -= BizInfo[bizid][bNalog];
	}
}

stock ShowBusinessInformation(playerid, bizid)
{
	new status[24];
	if(BizInfo[bizid][bLock] == 0) strcat(status,""c_green"Открыт");
