stock OnPlayerAmmoChange(playerid, newammo, oldammo)
{
	if(newammo < oldammo)
	{
		Shot[playerid]++;
		switch(GetPlayerWeapon(playerid))
		{
		case 24: if(Shot[playerid] >= 25 && Player[playerid][pDesertEagle] < 100) Player[playerid][pDesertEagle]++, Shot[playerid] = 0;
		case 23: if(Shot[playerid] >= 25 && Player[playerid][pSDPistol] < 100) Player[playerid][pSDPistol]++, Shot[playerid] = 0;
		case 25: if(Shot[playerid] >= 50 && Player[playerid][pShotGun] < 100) Player[playerid][pShotGun]++, Shot[playerid] = 0;
		case 29: if(Shot[playerid] >= 50 && Player[playerid][pMP5] < 100) Player[playerid][pMP5]++, Shot[playerid] = 0;
		case 30: if(Shot[playerid] >= 50 && Player[playerid][pAK47] < 100) Player[playerid][pAK47]++, Shot[playerid] = 0;
		case 31: if(Shot[playerid] >= 50 && Player[playerid][pM4] < 100) Player[playerid][pM4]++, Shot[playerid] = 0;
		}
	}
	return true;
}
stock GetPlayerID(string[])
{
	foreach(Player,i)
	{
		if(IsPlayerConnected(i) == 1)
		{
			if(strcmp(Player[i][pNames], string, true, strlen(string)) == 0) { return i; }
		}
	}
	return INVALID_PLAYER_ID;
}
stock SendClientMessageAH(color, string[])
{
	foreach(Player,i)
	{
		if(!IsPlayerConnected(i) || gPlayerLogged[i] == 0) continue;
		if(Player[i][pAdmin] > 0 || Player[i][pHelper] > 0) SendClientMessage(i, color, string);
	}
	return true;
}
