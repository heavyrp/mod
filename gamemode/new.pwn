		switch(params[0])
		{
		case 1: mestoz = "Заброшенный аэропорт";
		case 2: mestoz = "Дамба";
		case 3: mestoz = "Карьер";
		}
		GangZoneShowForPlayer(playerid,grove[params[0]], COLOR_GREEN);
		format(stringer1,sizeof(stringer1), "%s{FFFFFF} забила вам стрелу, цена которой является бизнес {4169E1}%s{FFFFFF}. Место встречи: %s.", GetMN(Player[playerid][pMember]),SBizzInfo[h][sMessage],mestoz);
		SendFamilyMessage(SBizzInfo[h][sbMafia],COLOR_WHITE,stringer1);
