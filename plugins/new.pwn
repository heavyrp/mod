	SetPVarInt(playerid, "RezervAmmo", 0);
	SetPVarInt(playerid, "GetStrike", 0);
	//
	if(gPlayerLogged[playerid] == 1) strmid(ExitInfo[playerid][ExitName],PlayerInfo[playerid][pNames], 0, strlen(PlayerInfo[playerid][pNames]), 32), ExitInfo[playerid][ExitTime] = GetTickCount();
	new query[128]; new stringer[144];
	format(query, sizeof(query), "UPDATE `accounts` SET `pOnline` = '0' WHERE `Name` = '%s'",PlayerInfo[playerid][pNames]);
	mysql_function_query(connects, query, false, "", "");
	ResetCarInfo(playerid);
	//
	if(PlayerInfo[playerid][pAdmin] > 0 && dostup[playerid] == 1) dostup[playerid] = 0;
	if(PlayerCuffed[playerid] == 2 && PlayerInfo[playerid][pWanted] > 0)
	{
		PlayerInfo[playerid][pJailed] = 1;
		PlayerInfo[playerid][pMestoJail] = 1;
		PlayerInfo[playerid][pJailTime] = 600*PlayerInfo[playerid][pWanted]+1800;
		format(strings,100,"<Warning> %s вышел с уровнем розыска %d при заходе будет посажен на %d секунд.",PlayerInfo[playerid][pNames],PlayerInfo[playerid][pWanted],PlayerInfo[playerid][pJailTime]);
		PlayerInfo[playerid][pWanted] = 0;
		SendFamilyMessage(1, 0xFEBC41AA, strings);
		SendFamilyMessage(10, 0xFEBC41AA, strings);
		SendFamilyMessage(2, 0xFEBC41AA, strings);
		SendFamilyMessage(21, 0xFEBC41AA, strings);
	}
	if(capt == 1 || capt == 10)
	{
		if(PlayerInfo[playerid][pMember] == 12 || PlayerInfo[playerid][pMember] == 13 || PlayerInfo[playerid][pMember] == 15 || PlayerInfo[playerid][pMember] == 17 || PlayerInfo[playerid][pMember] == 18)
		{
			TextDrawHideForPlayer(playerid, fon);
			TextDrawHideForPlayer(playerid, onecommand);
			TextDrawHideForPlayer(playerid, twocommand);
			TextDrawHideForPlayer(playerid, kolone);
			TextDrawHideForPlayer(playerid, koltwo);
			TextDrawHideForPlayer(playerid, timetext);
			TextDrawHideForPlayer(playerid, vremya);
			GangZoneStopFlashForPlayer(playerid, WarZoneID);
		}
	}
	CancelSelectTextDraw(playerid);
	TeleportTime[playerid] = 0;
	if(avtocar[playerid] != 0)
	{
		if(caroff[avtocar[playerid]] == 1) DestroyVehicle(avtocar[playerid]);
		avtocar[playerid] = 0;
		if(PlayerInfo[playerid][pSkilla] > 0) PlayerInfo[playerid][pSkilla]--;
	}
	if(tookmoped[playerid] != 0) DestroyVehicle(tookmoped[playerid]);
	if(PlayerInfo[playerid][pJob] == 2)
	{
		if(gcontract[playerid] == 1)
		{
			Delete3DTextLabel(Meh3d[GetPlayerVehicleID(playerid)]);
			gcontract[playerid] = 0;
		}
	}
	if(PlayerInfo[playerid][pPhousekey] != 255)
	{
		PlayerInfo[playerid][pProz] = PlayerInfo[playerid][pProz];
		if(caridhouse[playerid] != 0)
		{
			DestroyVehicle(caridhouse[playerid]);
			caridhouse[playerid] = 0;
		}
	}
	new playersip[16];
	new year, month,day;
	zips[playerid] = 0;
	if(MehJob[playerid] == 0)
	{
		AvtoInfo[0][Rab] -= 1;
		if(AvtoInfo[0][Rab] < 0)
		{
			AvtoInfo[0][Rab] = 0;
		}
	}
	MehJob[playerid] = 999;
	getdate(year, month, day);
	new hour, minute, second;
	gettime(hour, minute, second);
	GetPlayerIp(playerid,playersip,sizeof(playersip));
	if(IsPlayerAttachedObjectSlotUsed(playerid,1)) RemovePlayerAttachedObject(playerid,1);
	if(PlayerInfo[playerid][pJob] == 7) PlayerInfo[playerid][pJob] = 0;
	if(SelectChar[playerid] == INVALID_PLAYER_ID)
	{
		SetPlayerSkin(playerid, OldSkin[playerid]);
		PlayerInfo[playerid][pChar] = OldSkin[playerid];
		SelectChar[playerid] = 0;
	}

	DestroyProgressBar(eda[playerid]);
	eda[playerid] = INVALID_BAR_ID;
	if(PlayerPaintballing[playerid] != false)
	{
		PaintballPlayers -= 1;
		PlayerPaintballing[playerid] = false;
	}
	keys[playerid] = 0;
	tune[playerid] = 0;
	soglasen[playerid] = 0;
	FormaFrac[playerid] = 0;
	if(mesto[playerid] == 0)
	{
		AvtoInfo[0][Mesto] = 255;
	}
	tipsterOn[playerid] = false;
	tipsterr[playerid] = false;
	tipsterl[playerid] = false;
	tipsterfail[playerid] = false;
	tipsterl2[playerid] = false;
	tipsterlisten = -1;
	tipsters[playerid] = -1;
	SelectCharPlace[playerid] = 0;
	CharPrice[playerid] = 0;
	SelectCharID[playerid] = 0;
	SelectChar[playerid] = 0;
	PicCP[playerid] = 0;
	repairdvig[playerid] = 0;
	takephone[playerid] = 0;
	gag[playerid] = 0;
	PicCP[playerid] = 0;
	enterbiz[playerid] = 0;
	arenda[playerid] = 0;
	arenda[playerid] = 0;
	PlayerInfo[playerid][pDrugtime] = 0;
	PlayerInfo[playerid][pGunfbiTime] = 0;
	forma[playerid] = 0;
	proverkaforma[playerid] = 0;
	SetPlayerSkills(playerid);
	SelectCharPlace[playerid] = 0;
	CharPrice[playerid] = 0;
	SelectCharID[playerid] = 0;
	SelectChar[playerid] = 0;
	KillTimer(STimer[playerid]);
	if(TransportDuty[playerid] == 1) TaxiDrivers -= 1;
	if(PlayerInfo[playerid][pMember] == 4) Medics -= 1;
	if(PlayerInfo[playerid][pMember] == 1 || PlayerInfo[playerid][pMember] == 10 || PlayerInfo[playerid][pMember] == 21) Police +=1;
	else if(PlayerInfo[playerid][pJob] == 2) Mechanics -= 1;
	if(TransportCost[playerid] > 0 && TransportDriver[playerid] != INVALID_PLAYER_ID)
	{
		if(IsPlayerConnected(TransportDriver[playerid]))
		{
			TransportMoney[TransportDriver[playerid]] += TransportCost[playerid];
			TransportTime[TransportDriver[playerid]] = 0;
			TransportCost[TransportDriver[playerid]] = 0;
			format(stringer, sizeof(stringer), "~g~+%d",TransportCost[playerid]);
			GameTextForPlayer(TransportDriver[playerid], stringer, 5000, 1);
		}
	}
	OnPlayerUpdateRL(playerid);
	KickEx(playerid); // Кикаем игрока.
	return true;
